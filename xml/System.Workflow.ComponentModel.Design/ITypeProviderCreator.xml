        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Workflow.ComponentModel.Design.ITypeProviderCreator">
          <source>Defines a group of methods that classes can use to create a type provider for an activity or workflow designer.</source>
          <target state="translated">定義一組方法，讓類別可以用來為活動或工作流程設計工具建立型別提供者。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetLocalAssembly(System.Object)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph> for which to obtain the local assembly.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph>，為其取得本機組件。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetLocalAssembly(System.Object)">
          <source>Returns the local assembly associated with the specified object.</source>
          <target state="translated">傳回與指定物件相關聯的本機組件。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetLocalAssembly(System.Object)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Reflection.Assembly" /&gt;</ph> associated with <ph id="ph2">&lt;paramref name="obj" /&gt;</ph>.</source>
          <target state="translated">與 <ph id="ph1">&lt;see cref="T:System.Reflection.Assembly" /&gt;</ph> 相關聯的 <ph id="ph2">&lt;paramref name="obj" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetLocalAssembly(System.Object)">
          <source>GetLocalAssembly provides the assembly returned by the <ph id="ph1">&lt;xref:System.Workflow.ComponentModel.Compiler.ITypeProvider.LocalAssembly%2A&gt;</ph> property.</source>
          <target state="translated">GetLocalAssembly 會提供 <ph id="ph1">&lt;xref:System.Workflow.ComponentModel.Compiler.ITypeProvider.LocalAssembly%2A&gt;</ph> 屬性傳回的組件。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetTransientAssembly(System.Reflection.AssemblyName)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Reflection.AssemblyName" /&gt;</ph> for which to retrieve the transient assembly.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Reflection.AssemblyName" /&gt;</ph>，為其擷取暫時性組件。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetTransientAssembly(System.Reflection.AssemblyName)">
          <source>Returns the transient assembly associated with the specified assembly name.</source>
          <target state="translated">傳回與指定物件名稱相關聯的暫時性 (Transient) 組件。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetTransientAssembly(System.Reflection.AssemblyName)">
          <source>The transient <ph id="ph1">&lt;see cref="T:System.Reflection.Assembly" /&gt;</ph> associated with <ph id="ph2">&lt;paramref name="assemblyName" /&gt;</ph>.</source>
          <target state="translated">與 <ph id="ph1">&lt;see cref="T:System.Reflection.Assembly" /&gt;</ph><ph id="ph2">&lt;paramref name="assemblyName" /&gt;</ph>相關聯的暫時性 。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetTypeProvider(System.Object)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph> for which to return the type provider.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph>，為其傳回型別提供者。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetTypeProvider(System.Object)">
          <source>Returns the type provider associated with the specified object.</source>
          <target state="translated">傳回與指定物件相關聯的型別提供者。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetTypeProvider(System.Object)">
          <source>An <ph id="ph1">&lt;see cref="T:System.Workflow.ComponentModel.Compiler.ITypeProvider" /&gt;</ph> to perform type provider tasks for <ph id="ph2">&lt;paramref name="obj" /&gt;</ph>.</source>
          <target state="translated">為 <ph id="ph1">&lt;see cref="T:System.Workflow.ComponentModel.Compiler.ITypeProvider" /&gt;</ph><ph id="ph2">&lt;paramref name="obj" /&gt;</ph>執行型別提供者工作的 。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetTypeResolutionService(System.Object)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph> for which to retrieve the type resolution service.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph>，為其擷取型別解析服務。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetTypeResolutionService(System.Object)">
          <source>Returns a service that can retrieve an assembly or type by name for the specified object.</source>
          <target state="translated">傳回可以透過指定物件名稱擷取組件或型別的服務。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Design.ITypeProviderCreator.GetTypeResolutionService(System.Object)">
          <source>The <ph id="ph1">&lt;see cref="T:System.ComponentModel.Design.ITypeResolutionService" /&gt;</ph> to use to retrieve an assembly or type.</source>
          <target state="translated">用來擷取組件或型別的 <ph id="ph1">&lt;see cref="T:System.ComponentModel.Design.ITypeResolutionService" /&gt;</ph>。</target>       </trans-unit>
