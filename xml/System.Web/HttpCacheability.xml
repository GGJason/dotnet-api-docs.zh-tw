      <MemberSignature Language="DocId" Value="F:System.Web.HttpCacheability.NoCache" />
      <MemberSignature Language="VB.NET" Value="NoCache" />
      <MemberSignature Language="C++ CLI" Value="NoCache" />
      <MemberType>Field</MemberType>
