      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Linq.Mapping.AutoSync">
          <source>Instructs the runtime how to retrieve the value after an insert or update operation.</source>
          <target state="translated">指示執行階段如何在插入或更新作業之後擷取值。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Data.Linq.Mapping.AutoSync.Always">
          <source>Always returns the value.</source>
          <target state="translated">永遠傳回值。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Data.Linq.Mapping.AutoSync.Default">
          <source>Automatically selects the value.</source>
          <target state="translated">自動選取值。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Data.Linq.Mapping.AutoSync.Never">
          <source>Never returns the value.</source>
          <target state="translated">絕不傳回值。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Data.Linq.Mapping.AutoSync.OnInsert">
          <source>Returns the value only after an insert operation.</source>
          <target state="translated">只有在插入作業之後才會傳回值。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Data.Linq.Mapping.AutoSync.OnUpdate">
          <source>Returns the value only after an update operation.</source>
          <target state="translated">只有在更新作業之後才會傳回值。</target>       </trans-unit>
