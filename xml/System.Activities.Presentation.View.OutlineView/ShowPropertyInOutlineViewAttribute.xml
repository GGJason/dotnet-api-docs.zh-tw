    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute">
          <source>Represents the attributes when showing the property in outline view.</source>
          <target state="translated">表示在大綱檢視中顯示屬性時的相關屬性。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute.ChildNodePrefix">
          <source>Gets or sets the prefix for the child node.</source>
          <target state="translated">取得或設定子節點的前置字元。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute.ChildNodePrefix">
          <source>The prefix for the child node.</source>
          <target state="translated">子節點的前置詞。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute.CurrentPropertyVisible">
          <source>Gets or sets whether the current property value is visible.</source>
          <target state="translated">取得或設定目前屬性值是否為可見。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute.CurrentPropertyVisible">
          <source><ph id="ph1">&lt;see langword="True" /&gt;</ph> if the current property value is visible; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果目前的屬性值為可見的，則為 <ph id="ph1">&lt;see langword="True" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute.DuplicatedChildNodesVisible">
          <source>Gets or sets whether the duplicated child nodes are visible.</source>
          <target state="translated">取得或設定複製的子節點是否為可見。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Presentation.View.OutlineView.ShowPropertyInOutlineViewAttribute.DuplicatedChildNodesVisible">
          <source><ph id="ph1">&lt;see langword="True" /&gt;</ph> if the duplicated child nodes are visible; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果複製的子節點是可見的，則為 <ph id="ph1">&lt;see langword="True" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
