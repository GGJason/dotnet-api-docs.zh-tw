      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Description.ServiceMetadataExtension">
          <source>Provides the implementation for the metadata publishing protocols.</source>
          <target state="translated">為中繼資料發行通訊協定提供實作。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceModel.Description.ServiceMetadataExtension">
          <source>The <ph id="ph1">&lt;xref:System.ServiceModel.Description.ServiceMetadataExtension&gt;</ph> is configured and used by configuring a service with the <ph id="ph2">&lt;xref:System.ServiceModel.Description.ServiceMetadataBehavior?displayProperty=nameWithType&gt;</ph>.</source>
          <target state="translated">藉由 <ph id="ph1">&lt;xref:System.ServiceModel.Description.ServiceMetadataExtension&gt;</ph> 設定服務，即可設定和使用 <ph id="ph2">&lt;xref:System.ServiceModel.Description.ServiceMetadataBehavior?displayProperty=nameWithType&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceModel.Description.ServiceMetadataExtension">
          <source>You can also use the <ph id="ph1">&lt;xref:System.ServiceModel.Description.ServiceMetadataExtension&gt;</ph> class to get the service's metadata at runtime by accessing the <ph id="ph2">&lt;xref:System.ServiceModel.Description.ServiceMetadataExtension.Metadata%2A&gt;</ph> property.</source>
          <target state="translated">您也可以藉由存取 <ph id="ph1">&lt;xref:System.ServiceModel.Description.ServiceMetadataExtension&gt;</ph> 屬性，使用 <ph id="ph2">&lt;xref:System.ServiceModel.Description.ServiceMetadataExtension.Metadata%2A&gt;</ph> 類別在執行階段取得服務的中繼資料。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataExtension.#ctor">
          <source>Creates an instance of a <ph id="ph1">&lt;see cref="T:System.ServiceModel.Description.ServiceMetadataExtension" /&gt;</ph> object.</source>
          <target state="translated">建立 <ph id="ph1">&lt;see cref="T:System.ServiceModel.Description.ServiceMetadataExtension" /&gt;</ph> 物件的執行個體。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Description.ServiceMetadataExtension.Metadata">
          <source>Gets the service's metadata at runtime.</source>
          <target state="translated">取得執行階段的服務中繼資料。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Description.ServiceMetadataExtension.Metadata">
          <source>A <ph id="ph1">&lt;see cref="T:System.ServiceModel.Description.MetadataSet" /&gt;</ph> object that contains the service's metadata at runtime.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.ServiceModel.Description.MetadataSet" /&gt;</ph> 物件，包含執行階段的服務中繼資料。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Description.ServiceMetadataExtension.Metadata">
          <source>Changes in the returned collection are reflected in subsequent requests for the service metadata.</source>
          <target state="translated">傳回之集合中的變更會反映在服務中繼資料的後續要求中。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Description.ServiceMetadataExtension.SingleWsdl">
          <source>Gets the single WSDL used by the service.</source>
          <target state="translated">取得服務所使用的單一 WSDL。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Description.ServiceMetadataExtension.SingleWsdl">
          <source>The single WSDL used by the service.</source>
          <target state="translated">服務所使用的單一 WSDL。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataExtension.System#ServiceModel#IExtension&amp;lt;System#ServiceModel#ServiceHostBase&amp;gt;#Attach(System.ServiceModel.ServiceHostBase)">
          <source>The <ph id="ph1">&lt;see cref="T:System.ServiceModel.ServiceHostBase" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.ServiceModel.ServiceHostBase" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataExtension.System#ServiceModel#IExtension&amp;lt;System#ServiceModel#ServiceHostBase&amp;gt;#Attach(System.ServiceModel.ServiceHostBase)">
          <source>Enables an object to participate in custom behavior.</source>
          <target state="translated">讓物件能夠參與自訂的行為。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataExtension.System#ServiceModel#IExtension&amp;lt;System#ServiceModel#ServiceHostBase&amp;gt;#Attach(System.ServiceModel.ServiceHostBase)">
          <source>Called when the runtime attaches the extension to an instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.ServiceHostBase" /&gt;</ph> class.</source>
          <target state="translated">在執行階段將延伸附加到 <ph id="ph1">&lt;see cref="T:System.ServiceModel.ServiceHostBase" /&gt;</ph> 類別的執行個體時呼叫。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataExtension.System#ServiceModel#IExtension&amp;lt;System#ServiceModel#ServiceHostBase&amp;gt;#Attach(System.ServiceModel.ServiceHostBase)">
          <source>For internal use only.</source>
          <target state="translated">僅供內部使用。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataExtension.System#ServiceModel#IExtension&amp;lt;System#ServiceModel#ServiceHostBase&amp;gt;#Detach(System.ServiceModel.ServiceHostBase)">
          <source>The <ph id="ph1">&lt;see cref="T:System.ServiceModel.InstanceContext" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.ServiceModel.InstanceContext" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataExtension.System#ServiceModel#IExtension&amp;lt;System#ServiceModel#ServiceHostBase&amp;gt;#Detach(System.ServiceModel.ServiceHostBase)">
          <source>Enables an object to participate in custom behavior.</source>
          <target state="translated">讓物件能夠參與自訂的行為。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataExtension.System#ServiceModel#IExtension&amp;lt;System#ServiceModel#ServiceHostBase&amp;gt;#Detach(System.ServiceModel.ServiceHostBase)">
          <source>Called when the runtime detaches the extension from an instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.InstanceContext" /&gt;</ph> class.</source>
          <target state="translated">在執行階段從 <ph id="ph1">&lt;see cref="T:System.ServiceModel.InstanceContext" /&gt;</ph> 類別的執行個體中斷連結延伸時呼叫。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataExtension.System#ServiceModel#IExtension&amp;lt;System#ServiceModel#ServiceHostBase&amp;gt;#Detach(System.ServiceModel.ServiceHostBase)">
          <source>For internal use only.</source>
          <target state="translated">僅供內部使用。</target>       </trans-unit>
