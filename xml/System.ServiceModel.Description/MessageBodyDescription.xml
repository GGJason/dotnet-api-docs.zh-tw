      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Description.MessageBodyDescription">
          <source>Represents the body of a SOAP message.</source>
          <target state="translated">表示 SOAP 訊息的本文。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceModel.Description.MessageBodyDescription">
          <source>Use the <ph id="ph1">&lt;xref:System.ServiceModel.Description.MessageBodyDescription&gt;</ph> class to construct or modify a SOAP message body when added to the description tree.</source>
          <target state="translated">您可以使用 <ph id="ph1">&lt;xref:System.ServiceModel.Description.MessageBodyDescription&gt;</ph> 類別，在將 SOAP 訊息本文加入至描述樹狀目錄時，進行建構或修改。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.MessageBodyDescription.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Description.MessageBodyDescription" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.ServiceModel.Description.MessageBodyDescription" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Description.MessageBodyDescription.Parts">
          <source>Gets the parts for the body of the SOAP message.</source>
          <target state="translated">取得 SOAP 訊息本文的部分。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Description.MessageBodyDescription.Parts">
          <source>The body parts that you can inspect or modify.</source>
          <target state="translated">可以檢查或修改的本文部分。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Description.MessageBodyDescription.ReturnValue">
          <source>Gets or sets the portion of the SOAP message that contains the return value.</source>
          <target state="translated">取得或設定包含傳回值的部分 SOAP 訊息。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Description.MessageBodyDescription.ReturnValue">
          <source>The return value, if any.</source>
          <target state="translated">傳回值 (如果有的話)。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Description.MessageBodyDescription.WrapperName">
          <source>Gets or sets the name of the wrapper element inside the SOAP body element if there is one.</source>
          <target state="translated">取得或設定 SOAP 本文項目內部包裝函式項目的名稱 (如果有的話)。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Description.MessageBodyDescription.WrapperName">
          <source>The wrapper element if there is one.</source>
          <target state="translated">包裝函式項目 (如果有的話)。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Description.MessageBodyDescription.WrapperNamespace">
          <source>Gets or sets the namespace of the wrapper element in the SOAP body if there is one.</source>
          <target state="translated">取得或設定 SOAP 本文中包裝函式項目的命名空間 (如果有的話)。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Description.MessageBodyDescription.WrapperNamespace">
          <source>The namespace of the wrapper element if there is one.</source>
          <target state="translated">包裝函式項目的命名空間 (如果有的話)。</target>       </trans-unit>
