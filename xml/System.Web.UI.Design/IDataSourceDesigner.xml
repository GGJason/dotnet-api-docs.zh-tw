    <altmember cref="T:System.ComponentModel.DesignerAttribute" />
    <related type="Article" href="https://docs.microsoft.com/previous-versions/visualstudio/visual-studio-2010/37899azc(v=vs.100)"><span data-ttu-id="152c0-111">擴充設計階段支援</span><span class="sxs-lookup"><span data-stu-id="152c0-111">Extending Design-Time Support</span></span></related>
