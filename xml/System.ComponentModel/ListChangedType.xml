      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ComponentModel.ListChangedType">
          <source>Specifies how the list changed.</source>
          <target state="translated">指定清單變更的方式。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ComponentModel.ListChangedType">
          <source>Used by the <ph id="ph1">&lt;xref:System.ComponentModel.ListChangedEventArgs.ListChangedType%2A&gt;</ph> property of the <ph id="ph2">&lt;xref:System.ComponentModel.ListChangedEventArgs&gt;</ph> class to indicate the way an <ph id="ph3">&lt;xref:System.ComponentModel.IBindingList&gt;</ph> object changes.</source>
          <target state="translated">使用<ph id="ph1">&lt;xref:System.ComponentModel.ListChangedEventArgs.ListChangedType%2A&gt;</ph>屬性<ph id="ph2">&lt;xref:System.ComponentModel.ListChangedEventArgs&gt;</ph>類別，表示方式<ph id="ph3">&lt;xref:System.ComponentModel.IBindingList&gt;</ph>物件變更。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ComponentModel.ListChangedType">
          <source>For an example of using this class, see <bpt id="p1">[</bpt>Handling DataView Events<ept id="p1">](~/docs/framework/data/adonet/dataset-datatable-dataview/handling-dataview-events.md)</ept>.</source>
          <target state="translated">如需使用這個類別的範例，請參閱<bpt id="p1">[</bpt>處理 DataView 的事件<ept id="p1">](~/docs/framework/data/adonet/dataset-datatable-dataview/handling-dataview-events.md)</ept>。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.ItemAdded">
          <source>An item added to the list.</source>
          <target state="translated">加入至清單的項目。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.ItemAdded">
          <source><ph id="ph1">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.NewIndex" /&gt;</ph> contains the index of the item that was added.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.NewIndex" /&gt;</ph> 包含加入項目的索引。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.ItemChanged">
          <source>An item changed in the list.</source>
          <target state="translated">在清單中變更的項目。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.ItemChanged">
          <source><ph id="ph1">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.NewIndex" /&gt;</ph> contains the index of the item that was changed.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.NewIndex" /&gt;</ph> 包含變更項目的索引。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.ItemDeleted">
          <source>An item deleted from the list.</source>
          <target state="translated">從清單中刪除的項目。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.ItemDeleted">
          <source><ph id="ph1">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.NewIndex" /&gt;</ph> contains the index of the item that was deleted.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.NewIndex" /&gt;</ph> 包含刪除項目的索引。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.ItemMoved">
          <source>An item moved within the list.</source>
          <target state="translated">在清單中移動的項目。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.ItemMoved">
          <source><ph id="ph1">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.OldIndex" /&gt;</ph> contains the previous index for the item, whereas <ph id="ph2">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.NewIndex" /&gt;</ph> contains the new index for the item.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.OldIndex" /&gt;</ph> 包含先前的項目索引，而 <ph id="ph2">&lt;see cref="P:System.ComponentModel.ListChangedEventArgs.NewIndex" /&gt;</ph> 包含新的項目索引。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.PropertyDescriptorAdded">
          <source>A <ph id="ph1">&lt;see cref="T:System.ComponentModel.PropertyDescriptor" /&gt;</ph> was added, which changed the schema.</source>
          <target state="translated">已加入 <ph id="ph1">&lt;see cref="T:System.ComponentModel.PropertyDescriptor" /&gt;</ph>，其變更結構描述 (Schema)。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.PropertyDescriptorChanged">
          <source>A <ph id="ph1">&lt;see cref="T:System.ComponentModel.PropertyDescriptor" /&gt;</ph> was changed, which changed the schema.</source>
          <target state="translated">已變更 <ph id="ph1">&lt;see cref="T:System.ComponentModel.PropertyDescriptor" /&gt;</ph>，其變更結構描述。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.PropertyDescriptorDeleted">
          <source>A <ph id="ph1">&lt;see cref="T:System.ComponentModel.PropertyDescriptor" /&gt;</ph> was deleted, which changed the schema.</source>
          <target state="translated">已刪除 <ph id="ph1">&lt;see cref="T:System.ComponentModel.PropertyDescriptor" /&gt;</ph>，其變更結構描述。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.Reset">
          <source>Much of the list has changed.</source>
          <target state="translated">大部分的清單已變更。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.ListChangedType.Reset">
          <source>Any listening controls should refresh all their data from the list.</source>
          <target state="translated">任何接聽的控制項都應該從清單中重新整理它們的所有資料。</target>       </trans-unit>
