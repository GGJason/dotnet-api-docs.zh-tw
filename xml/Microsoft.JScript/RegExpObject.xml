      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.JScript.RegExpObject">
          <source>Represents a regular expression object.</source>
          <target state="translated">表示規則運算式物件。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:Microsoft.JScript.RegExpObject">
          <source>This class belongs to the built-in object model category.</source>
          <target state="translated">這個類別屬於內建物件模型分類。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:Microsoft.JScript.RegExpObject.global">
          <source>Gets a value indicating whether searches are performed with the global flag (<bpt id="p1">&lt;c&gt;</bpt>/g<ept id="p1">&lt;/c&gt;</ept>), matching all occurrences.</source>
          <target state="translated">取得值，這個值表示是否使用全域旗標 (<bpt id="p1">&lt;c&gt;</bpt>/g<ept id="p1">&lt;/c&gt;</ept>) 比對所有項目，來執行搜尋。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.JScript.RegExpObject.global">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if global searches are performed; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果執行全域搜尋，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:Microsoft.JScript.RegExpObject.ignoreCase">
          <source>Gets a value indicating whether searches are performed with the case-insensitive flag (<bpt id="p1">&lt;c&gt;</bpt>/i<ept id="p1">&lt;/c&gt;</ept>).</source>
          <target state="translated">取得值，這個值表示是否使用不區分大小寫的旗標 (<bpt id="p1">&lt;c&gt;</bpt>/i<ept id="p1">&lt;/c&gt;</ept>)，來執行搜尋。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.JScript.RegExpObject.ignoreCase">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if case-insensitive searches are performed; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果執行不區分大小寫的搜尋，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:Microsoft.JScript.RegExpObject.lastIndex">
          <source>Gets or sets the index of the last successful match.</source>
          <target state="translated">取得或設定最後一個成功比對的索引。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.JScript.RegExpObject.lastIndex">
          <source>The index of the last successful match, if there was one.</source>
          <target state="translated">最後一個成功比對的索引 (如果有的話)。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:Microsoft.JScript.RegExpObject.multiline">
          <source>Gets a value indicating whether searches are performed with the multiline flag (<bpt id="p1">&lt;c&gt;</bpt>/m<ept id="p1">&lt;/c&gt;</ept>).</source>
          <target state="translated">取得值，這個值表示是否使用多行旗標 (<bpt id="p1">&lt;c&gt;</bpt>/m<ept id="p1">&lt;/c&gt;</ept>)，來執行搜尋。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.JScript.RegExpObject.multiline">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if searches are performed in multiline mode; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果以多行模式執行搜尋，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="P:Microsoft.JScript.RegExpObject.source">
          <source>Gets the regular expression pattern that is represented by this object.</source>
          <target state="translated">取得這個物件表示的規則運算式模式。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.JScript.RegExpObject.source">
          <source>The regular expression pattern that is represented by this object.</source>
          <target state="translated">這個物件表示的規則運算式模式。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:Microsoft.JScript.RegExpObject.ToString">
          <source>Creates a string representation of this object that contains the regular expression pattern and flags.</source>
          <target state="translated">建立這個物件的字串表示，內含規則運算式模式和旗標。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:Microsoft.JScript.RegExpObject.ToString">
          <source>A string representation of this object.</source>
          <target state="translated">這個物件的字串表示。</target>       </trans-unit>
