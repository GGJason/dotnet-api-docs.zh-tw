    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Presentation.IActivityToolboxService">
          <source>Represents a service used to update Toolbox contents from a Designer.</source>
          <target state="translated">表示用於從「設計工具」更新工具箱內容的服務。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.AddCategory(System.String)">
          <source>A string that contains the name of the category to be added.</source>
          <target state="translated">包含要加入之類別名稱的字串。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.AddCategory(System.String)">
          <source>Adds a category to the toolbox.</source>
          <target state="translated">將類別加入至工具箱。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.AddItem(System.String,System.String)">
          <source>A string that contains the qualified name of the item to be added.</source>
          <target state="translated">包含要加入之項目合格名稱的字串。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.AddItem(System.String,System.String)">
          <source>A string that contains the name of the category to which the new item is added.</source>
          <target state="translated">包含要加入新項目之類別名稱的字串。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.AddItem(System.String,System.String)">
          <source>Adds an item to a specific category in the toolbox.</source>
          <target state="translated">將項目加入至工具箱中的特定類別。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.EnumCategories">
          <source>Gets an enumerator for all the categories in this toolbox.</source>
          <target state="translated">取得此工具箱中所有類別的列舉值。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.EnumCategories">
          <source>Returns <ph id="ph1">&lt;see cref="T:System.Collections.Generic.IList`1" /&gt;</ph>.</source>
          <target state="translated">傳回 <ph id="ph1">&lt;see cref="T:System.Collections.Generic.IList`1" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.EnumItems(System.String)">
          <source>A category under which all the items will be returned.</source>
          <target state="translated">傳回下方所有項目的類別。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.EnumItems(System.String)">
          <source>Gets an enumerator for all the items under the specified category in this toolbox.</source>
          <target state="translated">取得此工具箱中指定類別下所有項目的列舉值。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.EnumItems(System.String)">
          <source>Returns <ph id="ph1">&lt;see cref="T:System.Collections.Generic.IList`1" /&gt;</ph>.</source>
          <target state="translated">傳回 <ph id="ph1">&lt;see cref="T:System.Collections.Generic.IList`1" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.RemoveCategory(System.String)">
          <source>A string that contains the name of the category to be removed.</source>
          <target state="translated">包含要移除之類別名稱的字串。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.RemoveCategory(System.String)">
          <source>Removes a category from the toolbox.</source>
          <target state="translated">將類別從工具箱中移除。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.RemoveItem(System.String,System.String)">
          <source>A string that contains the qualified name of the item to be removed.</source>
          <target state="translated">包含要移除之項目合格名稱的字串。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.RemoveItem(System.String,System.String)">
          <source>A string that contains the name of the category from which the item is removed.</source>
          <target state="translated">包含要移除項目之類別名稱的字串。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.IActivityToolboxService.RemoveItem(System.String,System.String)">
          <source>Removes an item from a specific category in the toolbox.</source>
          <target state="translated">從工具箱中的特定類別移除項目。</target>       </trans-unit>
