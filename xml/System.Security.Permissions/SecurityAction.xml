        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Permissions.SecurityAction">
          <source>Specifies the security actions that can be performed using declarative security.</source>
          <target state="translated">指定可以使用宣告式安全性執行的安全性動作。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Permissions.SecurityAction">
