      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Permissions.StrongNamePublicKeyBlob">
          <source>Represents the public key information (called a blob) for a strong name.</source>
          <target state="translated">表示強式名稱的公開金鑰資訊 (稱為 Blob)。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Security.Permissions.StrongNamePublicKeyBlob">
          <source>This class cannot be inherited.</source>
          <target state="translated">這個類別無法被繼承。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Permissions.StrongNamePublicKeyBlob">
          <source>This class facilitates the use of strong names for identification purposes.</source>
          <target state="translated">這個類別可協助使用強式名稱做為識別用途。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Permissions.StrongNamePublicKeyBlob">
          <source>For a complete description of strong names, see <ph id="ph1">&lt;xref:System.Security.Policy.StrongName&gt;</ph>.</source>
          <target state="translated">如需強式名稱的完整說明，請參閱<ph id="ph1">&lt;xref:System.Security.Policy.StrongName&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.#ctor(System.Byte[])">
          <source>The array of bytes representing the raw public key data.</source>
          <target state="translated">位元組的陣列，表示未經處理的公開金鑰 (Public Key) 資料。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.#ctor(System.Byte[])">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Security.Permissions.StrongNamePublicKeyBlob" /&gt;</ph> class with raw bytes of the public key blob.</source>
          <target state="translated">使用公開金鑰 Blob 未經處理的位元組來初始化 <ph id="ph1">&lt;see cref="T:System.Security.Permissions.StrongNamePublicKeyBlob" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.#ctor(System.Byte[])">
          <source>The <ph id="ph1">&lt;paramref name="publicKey" /&gt;</ph> parameter is <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="publicKey" /&gt;</ph> 參數為 <ph id="ph2">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.Equals(System.Object)">
          <source>An object containing a public key blob.</source>
          <target state="translated">物件，包含公開金鑰 Blob。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.Equals(System.Object)">
          <source>Gets or sets a value indicating whether the current public key blob is equal to the specified public key blob.</source>
          <target state="translated">取得或設定數值，表示目前的公開金鑰 Blob 是否等於指定的公開金鑰 Blob。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.Equals(System.Object)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the public key blob of the current object is equal to the public key blob of the <ph id="ph2">&lt;paramref name="o" /&gt;</ph> parameter; otherwise, <ph id="ph3">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果目前物件的公開金鑰 Blob 等於 <ph id="ph2">&lt;paramref name="o" /&gt;</ph> 參數的公開金鑰 Blob，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph3">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.GetHashCode">
          <source>Returns a hash code based on the public key.</source>
          <target state="translated">傳回以公開金鑰為基礎的雜湊程式碼。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.GetHashCode">
          <source>The hash code based on the public key.</source>
          <target state="translated">以公開金鑰為基礎的雜湊程式碼。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.GetHashCode">
          <source>Hash functions map binary strings of an arbitrary length to small binary strings of a fixed length, known as hash values.</source>
          <target state="translated">雜湊函式對應固定長度，又稱為雜湊值的小型二進位的字串任意長度的二進位字串。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.ToString">
          <source>Creates and returns a string representation of the public key blob.</source>
          <target state="translated">建立並傳回公開金鑰 Blob 的字串表示。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.StrongNamePublicKeyBlob.ToString">
          <source>A hexadecimal version of the public key blob.</source>
          <target state="translated">公開金鑰 Blob 的十六進位版本。</target>       </trans-unit>
