      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.ModelBinding.SimpleValueProvider">
          <source>Represents a value provider for single key/value lookups.</source>
          <target state="translated">表示單一索引鍵/值查詢的值提供者。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.ModelBinding.SimpleValueProvider">
          <source>This class does not contain the actual look-up mechanism; it delegates that to derived classes using the <ph id="ph1">&lt;xref:System.Web.ModelBinding.SimpleValueProvider.FetchValue%2A&gt;</ph> method.</source>
          <target state="translated">這個類別不包含實際的查詢機制。它衍生的類別使用委派<ph id="ph1">&lt;xref:System.Web.ModelBinding.SimpleValueProvider.FetchValue%2A&gt;</ph>方法。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.ModelBinding.SimpleValueProvider">
          <source>Examples of simple value providers are the <ph id="ph1">&lt;xref:System.Web.ModelBinding.ViewStateValueProvider&gt;</ph> class and the <ph id="ph2">&lt;xref:System.Web.ModelBinding.ControlValueProvider&gt;</ph> class.</source>
          <target state="translated">簡單的值提供者的範例包括<ph id="ph1">&lt;xref:System.Web.ModelBinding.ViewStateValueProvider&gt;</ph>類別和<ph id="ph2">&lt;xref:System.Web.ModelBinding.ControlValueProvider&gt;</ph>類別。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="T:System.Web.ModelBinding.SimpleValueProvider">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.SimpleValueProvider" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.SimpleValueProvider" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.#ctor(System.Web.ModelBinding.ModelBindingExecutionContext)">
          <source>The execution context.</source>
          <target state="translated">執行內容。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.#ctor(System.Web.ModelBinding.ModelBindingExecutionContext)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.SimpleValueProvider" /&gt;</ph> class by using the specified execution context.</source>
          <target state="translated">使用指定的執行內容，初始化 <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.SimpleValueProvider" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.#ctor(System.Web.ModelBinding.ModelBindingExecutionContext,System.Globalization.CultureInfo)">
          <source>The execution context.</source>
          <target state="translated">執行內容。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.#ctor(System.Web.ModelBinding.ModelBindingExecutionContext,System.Globalization.CultureInfo)">
          <source>The culture information.</source>
          <target state="translated">文化特性資訊。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.#ctor(System.Web.ModelBinding.ModelBindingExecutionContext,System.Globalization.CultureInfo)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.SimpleValueProvider" /&gt;</ph> class by using the specified execution context and culture information.</source>
          <target state="translated">使用指定的執行內容和文化特性資訊，初始化 <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.SimpleValueProvider" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.ContainsPrefix(System.String)">
          <source>The prefix.</source>
          <target state="translated">前置詞。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.ContainsPrefix(System.String)">
          <source>Gets a value that indicates whether the value provider contains the specified prefix.</source>
          <target state="translated">取得值，指出值提供者是否包含指定的前置詞。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.ContainsPrefix(System.String)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the value provider contains the specified prefix; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果值提供者包含指定的前置詞，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.ContainsPrefix(System.String)">
          <source>The <ph id="ph1">&lt;paramref name="prefix" /&gt;</ph> parameter is <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="prefix" /&gt;</ph> 參數為 <ph id="ph2">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.FetchValue(System.String)">
          <source>The key of the value to retrieve.</source>
          <target state="translated">要擷取之值的索引鍵。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.FetchValue(System.String)">
          <source>When implemented in a derived class, returns the value for the specified key.</source>
          <target state="translated">在衍生類別中實作時，傳回指定的索引鍵的值。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.FetchValue(System.String)">
          <source>The value.</source>
          <target state="translated">數值。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.GetValue(System.String)">
          <source>The key of the value to retrieve.</source>
          <target state="translated">要擷取之值的索引鍵。</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.GetValue(System.String)">
          <source>Calls the <ph id="ph1">&lt;see cref="M:System.Web.ModelBinding.SimpleValueProvider.FetchValue(System.String)" /&gt;</ph> method and then uses the returned value to instantiate a new instance of the <ph id="ph2">&lt;see cref="T:System.Web.ModelBinding.ValueProviderResult" /&gt;</ph> class.</source>
          <target state="translated">呼叫 <ph id="ph1">&lt;see cref="M:System.Web.ModelBinding.SimpleValueProvider.FetchValue(System.String)" /&gt;</ph> 方法，然後使用傳回值執行個體化 <ph id="ph2">&lt;see cref="T:System.Web.ModelBinding.ValueProviderResult" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.GetValue(System.String)">
          <source>The value.</source>
          <target state="translated">數值。</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.SimpleValueProvider.GetValue(System.String)">
          <source>The <ph id="ph1">&lt;paramref name="key" /&gt;</ph> parameter is <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="key" /&gt;</ph> 參數為 <ph id="ph2">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve" uid="P:System.Web.ModelBinding.SimpleValueProvider.ModelBindingExecutionContext">
          <source>Gets the execution context.</source>
          <target state="translated">取得執行內容。</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.ModelBinding.SimpleValueProvider.ModelBindingExecutionContext">
          <source>The execution context.</source>
          <target state="translated">執行內容。</target>       </trans-unit>
