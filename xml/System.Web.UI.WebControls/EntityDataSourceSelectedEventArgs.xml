    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs">
          <source>Provides data for the <ph id="ph1">&lt;see cref="E:System.Web.UI.WebControls.EntityDataSource.Selected" /&gt;</ph> event.</source>
          <target state="translated">提供 <ph id="ph1">&lt;see cref="E:System.Web.UI.WebControls.EntityDataSource.Selected" /&gt;</ph> 事件的資料。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.Context">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Data.Objects.ObjectContext" /&gt;</ph> used by the data source.</source>
          <target state="translated">取得資料來源所使用的 <ph id="ph1">&lt;see cref="T:System.Data.Objects.ObjectContext" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.Context">
          <source>The context used by the data source.</source>
          <target state="translated">資料來源所使用的內容。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.Exception">
          <source>Gets the exception that was thrown during the query.</source>
          <target state="translated">取得在查詢期間擲回的例外狀況 (Exception)。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.Exception">
          <source>The exception, if an error occurred; otherwise, <ph id="ph1">&lt;see langword="null" /&gt;</ph> reference (<ph id="ph2">&lt;see langword="Nothing" /&gt;</ph> in Visual Basic).</source>
          <target state="translated">如果發生錯誤則為例外狀況，否則為 <ph id="ph1">&lt;see langword="null" /&gt;</ph> 參考 (在 Visual Basic 中為 <ph id="ph2">&lt;see langword="Nothing" /&gt;</ph>)。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.ExceptionHandled">
          <source>Gets or sets a value that indicates whether the exception was handled and that it should not be thrown again.</source>
          <target state="translated">取得或設定值，這個值表示是否處理了例外狀況以及是否不應再度擲回此例外狀況。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.ExceptionHandled">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the exception was handled; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果例外狀況已處理則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.Results">
          <source>Gets the objects returned by the query.</source>
          <target state="translated">取得查詢傳回的物件。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.Results">
          <source>The objects returned by the query.</source>
          <target state="translated">查詢所傳回的物件。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.SelectArguments">
          <source>Gets values that determine how the data is returned.</source>
          <target state="translated">取得決定資料之傳回方式的值。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.SelectArguments">
          <source>An object that contains values for determining how the data is returned.</source>
          <target state="translated">物件，包含決定資料之傳回方式的值。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.TotalRowCount">
          <source>Gets the total number of objects returned by the query.</source>
          <target state="translated">取得查詢傳回的物件總數。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.WebControls.EntityDataSourceSelectedEventArgs.TotalRowCount">
          <source>The total number of objects returned.</source>
          <target state="translated">傳回的物件總數。</target>       </trans-unit>
