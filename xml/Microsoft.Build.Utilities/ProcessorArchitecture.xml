      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Utilities.ProcessorArchitecture">
          <source>Determines the correct tool in the Toolset that MSBuild should use, based on the current system's processor architecture.</source>
          <target state="translated">根據目前系統的處理器架構，決定在工具組中 MSBuild 應使用的正確工具。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.Build.Utilities.ProcessorArchitecture">
          <source>The Toolset contains several tools, such as compilers, that are used in different situations.</source>
          <target state="translated">工具組包含數個工具，例如編譯器，用於不同的情況。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.Build.Utilities.ProcessorArchitecture">
          <source><ph id="ph1">&lt;xref:System.Reflection.ProcessorArchitecture&gt;</ph> determines which of these tools should be used based on the current system's architecture or CPU, such as x86, AMD64, and so forth.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Reflection.ProcessorArchitecture&gt;</ph> 決定哪些這些工具應該使用根據目前系統的架構或 CPU、 x86、 AMD64，依此類推。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.Build.Utilities.ProcessorArchitecture">
          <source>For more information on Toolsets, see <bpt id="p1">[</bpt>Standard and Custom Toolset Configurations<ept id="p1">](http://msdn.microsoft.com/library/15a048c8-5ad3-448e-b6e9-e3c5d7147ed2)</ept>.</source>
          <target state="translated">如需工具組的詳細資訊，請參閱<bpt id="p1">[</bpt>標準和自訂工具組組態<ept id="p1">](http://msdn.microsoft.com/library/15a048c8-5ad3-448e-b6e9-e3c5d7147ed2)</ept>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Utilities.ProcessorArchitecture.AMD64">
          <source>Represents the AMD64 processor.</source>
          <target state="translated">代表 AMD64 處理器。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Utilities.ProcessorArchitecture.ARM">
          <source>Represents an ARM processor.</source>
          <target state="translated">代表 ARM 處理器。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Utilities.ProcessorArchitecture.CurrentProcessArchitecture">
          <source>Gets the current processor architecture of the system.</source>
          <target state="translated">取得目前系統的處理器架構。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Utilities.ProcessorArchitecture.CurrentProcessArchitecture">
          <source>A string containing the processor architecture value.</source>
          <target state="translated">包含處理器架構值的字串。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Utilities.ProcessorArchitecture.IA64">
          <source>Represents the IA64 processor.</source>
          <target state="translated">表示 IA64 處理器。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Utilities.ProcessorArchitecture.MSIL">
          <source>Represents MSIL.</source>
          <target state="translated">表示 MSIL。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Utilities.ProcessorArchitecture.X86">
          <source>Represents an x86 processor.</source>
          <target state="translated">代表 x86 處理器。</target>       </trans-unit>
