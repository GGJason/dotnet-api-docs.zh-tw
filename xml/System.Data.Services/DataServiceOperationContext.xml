    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Services.DataServiceOperationContext">
          <source>Represents the current operation being processed.</source>
          <target state="translated">表示正在處理的目前作業。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Data.Services.DataServiceOperationContext.AbsoluteRequestUri">
          <source>Get the request URI for the current operation.</source>
          <target state="translated">取得目前作業的要求 URI。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.DataServiceOperationContext.AbsoluteRequestUri">
          <source>The <ph id="ph1">&lt;see cref="T:System.Uri" /&gt;</ph> of the operation.</source>
          <target state="translated">作業的 <ph id="ph1">&lt;see cref="T:System.Uri" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.Data.Services.DataServiceOperationContext.AbsoluteServiceUri">
          <source>Gets the base service URI for the request.</source>
          <target state="translated">取得要求的基礎服務 URI。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.DataServiceOperationContext.AbsoluteServiceUri">
          <source>The base <ph id="ph1">&lt;see cref="T:System.Uri" /&gt;</ph> of the operation.</source>
          <target state="translated">作業的基底 <ph id="ph1">&lt;see cref="T:System.Uri" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Data.Services.DataServiceOperationContext.IsBatchRequest">
          <source>Gets a value that indicates whether the current operation is part of a batch request.</source>
          <target state="translated">取得值，該值指出目前作業是否為批次要求的一部分。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.DataServiceOperationContext.IsBatchRequest">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> when the operation is part of a batch request; otherwise <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果作業屬於批次要求的一部分則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Data.Services.DataServiceOperationContext.RequestHeaders">
          <source>Gets the request headers for the current operation.</source>
          <target state="translated">取得目前作業的要求標頭。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.DataServiceOperationContext.RequestHeaders">
          <source>A <ph id="ph1">&lt;see cref="T:System.Net.WebHeaderCollection" /&gt;</ph> object that contains the request headers.</source>
          <target state="translated">包含要求標頭的 <ph id="ph1">&lt;see cref="T:System.Net.WebHeaderCollection" /&gt;</ph> 物件。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Data.Services.DataServiceOperationContext.RequestMethod">
          <source>Gets the HTTP request method for the operation</source>
          <target state="translated">取得作業的 HTTP 要求方法。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.DataServiceOperationContext.RequestMethod">
          <source>The HTTP request method.</source>
          <target state="translated">HTTP 要求方法。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.Data.Services.DataServiceOperationContext.ResponseHeaders">
          <source>Gets the response headers for the current operation.</source>
          <target state="translated">取得目前作業的回應標頭。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.DataServiceOperationContext.ResponseHeaders">
          <source>A <ph id="ph1">&lt;see cref="T:System.Net.WebHeaderCollection" /&gt;</ph> object that contains the response headers.</source>
          <target state="translated">包含回應標頭的 <ph id="ph1">&lt;see cref="T:System.Net.WebHeaderCollection" /&gt;</ph> 物件。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="P:System.Data.Services.DataServiceOperationContext.ResponseStatusCode">
          <source>Gets or sets the status code of the response.</source>
          <target state="translated">取得或設定回應的狀態碼。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.DataServiceOperationContext.ResponseStatusCode">
          <source>The status code of the operation response.</source>
          <target state="translated">作業回應的狀態碼。</target>       </trans-unit>
