      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView">
          <source>Provides a design-time view of hierarchical data for the <ph id="ph1">&lt;see cref="T:System.Web.SiteMap" /&gt;</ph> class.</source>
          <target state="translated">為 <ph id="ph1">&lt;see cref="T:System.Web.SiteMap" /&gt;</ph> 類別提供階層式資料的設計階段檢視。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView.#ctor(System.Web.UI.Design.WebControls.SiteMapDataSourceDesigner,System.String)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.SiteMapDataSourceDesigner" /&gt;</ph> that is the designer for the associated <ph id="ph2">&lt;see cref="T:System.Web.UI.WebControls.SiteMapDataSource" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.SiteMapDataSourceDesigner" /&gt;</ph>，關聯 <ph id="ph2">&lt;see cref="T:System.Web.UI.WebControls.SiteMapDataSource" /&gt;</ph> 的設計工具。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView.#ctor(System.Web.UI.Design.WebControls.SiteMapDataSourceDesigner,System.String)">
          <source>An XPath query that defines the block of data to use for the view.</source>
          <target state="translated">XPath 查詢，定義要用於檢視的資料區塊。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView.#ctor(System.Web.UI.Design.WebControls.SiteMapDataSourceDesigner,System.String)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView.GetDesignTimeData(System.Boolean@)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> to indicate that the returned data is sample data; <ph id="ph2">&lt;see langword="false" /&gt;</ph> to indicate that the returned data is real data.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph> 表示傳回的資料是資料範例，<ph id="ph2">&lt;see langword="false" /&gt;</ph> 表示傳回的資料是實際資料。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView.GetDesignTimeData(System.Boolean@)">
          <source>Generates design-time data that matches the schema of the associated data source control and returns a value indicating whether the data is sample data or real data.</source>
          <target state="translated">產生設計階段資料，該資料符合關聯資料來源控制項的結構描述，並傳回值，指出資料是資料範例還是實際資料。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView.GetDesignTimeData(System.Boolean@)">
          <source>Design-time data that matches the schema of the associated data source control.</source>
          <target state="translated">符合相關聯資料來源控制項之結構描述的設計階段資料。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView.Schema">
          <source>Gets a schema that describes the data source view that is represented by this view object.</source>
          <target state="translated">取得結構描述，其描述這個檢視物件所代表的資料來源檢視。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.Design.WebControls.SiteMapDesignerHierarchicalDataSourceView.Schema">
          <source>A schema that describes the data source view that is represented by this view object.</source>
          <target state="translated">結構描述，這個結構描述會說明由這個檢視物件表示的資料來源檢視。</target>       </trans-unit>
