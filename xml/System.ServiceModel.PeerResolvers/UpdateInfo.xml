        <param name="registrationId">
          <see cref="T:System.Guid" />，可指定要更新之註冊項目的唯一 ID。</param>
        <param name="client">
          <see cref="T:System.Guid" />，可指定對等用戶端的唯一 ID，該用戶端已送出更新的註冊項目資訊。</param>
        <param name="meshId">
