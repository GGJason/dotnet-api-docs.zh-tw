        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.IdentityModel.Protocols.WSTrust.Status">
          <source>Represents the result of a WS-Trust Validation request.</source>
          <target state="translated">表示 Ws-Trust 驗證要求的結果。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.IdentityModel.Protocols.WSTrust.Status">
          <source>Represents the contents of the wst:Status element.</source>
          <target state="translated">代表 wst:Status 項目的內容。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.IdentityModel.Protocols.WSTrust.Status">
          <source>This required element contains the result of a WS-Trust Validation request.</source>
          <target state="translated">此必要項目包含 Ws-trust 驗證要求的結果。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.IdentityModel.Protocols.WSTrust.Status">
          <source>For more information about the element that this class represents, see the version of the WS-Trust specification that applies to your scenario.</source>
          <target state="translated">如需此類別代表的項目詳細資訊，請參閱適用於您案例的 Ws-trust 規格的版本。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Protocols.WSTrust.Status.#ctor(System.String,System.String)">
          <source>A string that contains the status code URI.</source>
          <target state="translated">包含狀態碼 URI 的字串。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Protocols.WSTrust.Status.#ctor(System.String,System.String)">
          <source>A string that contains a human-readable reason.</source>
          <target state="translated">包含人類看得懂的原因的字串。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Protocols.WSTrust.Status.#ctor(System.String,System.String)">
          <source>The reason is optional; can be <ph id="ph1">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated">原因是選擇性的；可以是 <ph id="ph1">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
