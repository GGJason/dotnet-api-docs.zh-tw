      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Xps.Serialization.XpsSerializerFactory">
          <source>Creates and provides information about XML Paper Specification (XPS) serializers.</source>
          <target state="translated">建立及提供與 XML 文件規格 (XPS) 序列化程式相關的資訊。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Windows.Xps.Serialization.XpsSerializerFactory.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Windows.Xps.Serialization.XpsSerializerFactory" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.Windows.Xps.Serialization.XpsSerializerFactory" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Windows.Xps.Serialization.XpsSerializerFactory.CreateSerializerWriter(System.IO.Stream)">
          <source>The output stream that the returned serializer is to write to.</source>
          <target state="translated">要寫入所傳回序列化程式中的輸出資料流。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Windows.Xps.Serialization.XpsSerializerFactory.CreateSerializerWriter(System.IO.Stream)">
          <source>Creates a <ph id="ph1">&lt;see cref="T:System.Windows.Documents.Serialization.SerializerWriter" /&gt;</ph> that outputs <ph id="ph2">[!INCLUDE[TLA2#tla_xps](~/includes/tla2sharptla-xps-md.md)]</ph> content to a specified <ph id="ph3">&lt;see cref="T:System.IO.Stream" /&gt;</ph>.</source>
          <target state="translated">建立會將 <ph id="ph2">[!INCLUDE[TLA2#tla_xps](~/includes/tla2sharptla-xps-md.md)]</ph> 內容輸出至所指定 <ph id="ph1">&lt;see cref="T:System.Windows.Documents.Serialization.SerializerWriter" /&gt;</ph> 的 <ph id="ph3">&lt;see cref="T:System.IO.Stream" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Windows.Xps.Serialization.XpsSerializerFactory.CreateSerializerWriter(System.IO.Stream)">
          <source>An output writer that serializes <ph id="ph1">[!INCLUDE[TLA2#tla_xps](~/includes/tla2sharptla-xps-md.md)]</ph> content to the specified <ph id="ph2">&lt;paramref name="stream" /&gt;</ph>.</source>
          <target state="translated">將 <ph id="ph1">[!INCLUDE[TLA2#tla_xps](~/includes/tla2sharptla-xps-md.md)]</ph> 內容序列化為指定之 <ph id="ph2">&lt;paramref name="stream" /&gt;</ph> 的輸出寫入器。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.DefaultFileExtension">
          <source>Gets the standard file name extension for <ph id="ph1">[!INCLUDE[TLA2#tla_xps](~/includes/tla2sharptla-xps-md.md)]</ph> documents.</source>
          <target state="translated">取得 <ph id="ph1">[!INCLUDE[TLA2#tla_xps](~/includes/tla2sharptla-xps-md.md)]</ph> 文件的標準副檔名。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.DefaultFileExtension">
          <source>The standard file name extension (including the leading period) for <ph id="ph1">[!INCLUDE[TLA2#tla_xps](~/includes/tla2sharptla-xps-md.md)]</ph> documents.</source>
          <target state="translated"><ph id="ph1">[!INCLUDE[TLA2#tla_xps](~/includes/tla2sharptla-xps-md.md)]</ph> 文件的標準副檔名 (包括前置句號)。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.DefaultFileExtension">
          <source>This method always returns the string ".xps".</source>
          <target state="translated">這個方法一律會傳回字串".xps"。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.DisplayName">
          <source>Gets the public name for the serializers that the factory produces.</source>
          <target state="translated">取得處理站產生之序列化程式的公用名稱。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.DisplayName">
          <source>The public name for serializers that the factory produces.</source>
          <target state="translated">處理站產生之序列化程式的公用名稱。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.DisplayName">
          <source>This method always returns the string "XPS Serializer".</source>
          <target state="translated">這個方法一律會傳回字串"XPS 序列化程式 」。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.ManufacturerName">
          <source>Gets the manufacturer's name for serializers that the factory produces.</source>
          <target state="translated">取得處理站產生之序列化程式的製造商名稱。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.ManufacturerName">
          <source>The manufacturer's name.</source>
          <target state="translated">製造商的名稱。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.ManufacturerName">
          <source>This method always returns the string "Microsoft".</source>
          <target state="translated">這個方法一律會傳回字串"Microsoft"。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.ManufacturerWebsite">
          <source>Gets the manufacturer's Web address for serializers that the factory produces.</source>
          <target state="translated">取得處理站產生之序列化程式的製造商網址。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.ManufacturerWebsite">
          <source>The manufacturer's Web site.</source>
          <target state="translated">製造商的網址。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializerFactory.ManufacturerWebsite">
          <source>This method always returns the <ph id="ph1">[!INCLUDE[TLA#tla_uri](~/includes/tlasharptla-uri-md.md)]</ph> "<ph id="ph2">http://www.microsoft.com</ph>".</source>
          <target state="translated">這個方法一律會傳回<ph id="ph1">[!INCLUDE[TLA#tla_uri](~/includes/tlasharptla-uri-md.md)]</ph>"<ph id="ph2">http://www.microsoft.com</ph>"。</target>       </trans-unit>
