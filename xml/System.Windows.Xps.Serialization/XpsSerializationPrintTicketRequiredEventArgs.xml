      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs">
          <source>Provides data for the <ph id="ph1">&lt;see cref="E:System.Windows.Xps.Serialization.XpsSerializationManager.XpsSerializationPrintTicketRequired" /&gt;</ph> event.</source>
          <target state="translated">提供 <ph id="ph1">&lt;see cref="E:System.Windows.Xps.Serialization.XpsSerializationManager.XpsSerializationPrintTicketRequired" /&gt;</ph> 事件的資料。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.#ctor(System.Windows.Xps.Serialization.PrintTicketLevel,System.Int32)">
          <source>An object that specifies the scope of the <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph>.</source>
          <target state="translated">指定 <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph> 之範圍的物件。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.#ctor(System.Windows.Xps.Serialization.PrintTicketLevel,System.Int32)">
          <source>Either the total number of pages or the number of documents in the print job.</source>
          <target state="translated">列印工作中的頁面總數或文件數目。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.#ctor(System.Windows.Xps.Serialization.PrintTicketLevel,System.Int32)">
          <source>Which applies depends on the scope of the <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph>.</source>
          <target state="translated">是頁面總數還是文件數目取決於 <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph> 的範圍。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.#ctor(System.Windows.Xps.Serialization.PrintTicketLevel,System.Int32)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.PrintTicket">
          <source>Gets or sets the <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph> that is used in the writing operation.</source>
          <target state="translated">取得或設定用於寫入作業的 <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.PrintTicket">
          <source>A <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph> that defines how the printer handles a print job.</source>
          <target state="translated">定義印表機如何處理列印工作的 <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.PrintTicketLevel">
          <source>Gets a value that indicates the scope of the <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph>.</source>
          <target state="translated">取得值，這個值表示 <ph id="ph1">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph> 的範圍。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.PrintTicketLevel">
          <source>A <ph id="ph1">&lt;see cref="T:System.Windows.Xps.Serialization.PrintTicketLevel" /&gt;</ph> value that indicates the scope of the <ph id="ph2">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Windows.Xps.Serialization.PrintTicketLevel" /&gt;</ph> 值，表示 <ph id="ph2">&lt;see cref="T:System.Printing.PrintTicket" /&gt;</ph> 的範圍。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.Sequence">
          <source>Gets either the total number of pages or the number of documents in the print job.</source>
          <target state="translated">取得列印工作中的頁面總數或文件數目。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.Sequence">
          <source>An <ph id="ph1">&lt;see cref="T:System.Int32" /&gt;</ph> that represents the total number of pages or the number of documents.</source>
          <target state="translated">表示頁面總數或文件數目的 <ph id="ph1">&lt;see cref="T:System.Int32" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.Sequence">
          <source>Whether this property represents pages or documents depends on the scope (for example, single document or multiple documents) of the <ph id="ph1">&lt;xref:System.Printing.PrintTicket&gt;</ph>.</source>
          <target state="translated">這個屬性是代表頁面或文件範圍而定 （例如，單一文件或多個文件） <ph id="ph1">&lt;xref:System.Printing.PrintTicket&gt;</ph>。</target>       </trans-unit>
