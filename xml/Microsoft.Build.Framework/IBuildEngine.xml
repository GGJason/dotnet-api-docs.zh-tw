        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Framework.IBuildEngine">
          <source>Provides a way for task authors to use a subset of the functionality of the MSBuild engine.</source>
          <target state="translated">可讓工作作者使用 MSBuild 引擎的一小部分功能。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Framework.IBuildEngine.BuildProjectFile(System.String,System.String[],System.Collections.IDictionary,System.Collections.IDictionary)">
          <source>The name of the project file to build.</source>
          <target state="translated">要建置的專案檔名稱。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Framework.IBuildEngine.BuildProjectFile(System.String,System.String[],System.Collections.IDictionary,System.Collections.IDictionary)">
          <source>The names of the target in the project to build.</source>
          <target state="translated">在要建置之專案中的目標名稱。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Framework.IBuildEngine.BuildProjectFile(System.String,System.String[],System.Collections.IDictionary,System.Collections.IDictionary)">
          <source>Separate multiple targets with a semicolon (;).</source>
          <target state="translated">使用分號 (;) 分隔多個目標。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Framework.IBuildEngine.BuildProjectFile(System.String,System.String[],System.Collections.IDictionary,System.Collections.IDictionary)">
          <source>An <ph id="ph1">&lt;see cref="T:System.Collections.IDictionary" /&gt;</ph> of additional global properties to apply to the project.</source>
          <target state="translated">要套用至專案之其他全域屬性的 <ph id="ph1">&lt;see cref="T:System.Collections.IDictionary" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Framework.IBuildEngine.BuildProjectFile(System.String,System.String[],System.Collections.IDictionary,System.Collections.IDictionary)">
          <source>The key and value must be <ph id="ph1">&lt;see langword="String" /&gt;</ph> data types.</source>
          <target state="translated">索引鍵和值都必須是 <ph id="ph1">&lt;see langword="String" /&gt;</ph> 資料類型。</target>       </trans-unit>
