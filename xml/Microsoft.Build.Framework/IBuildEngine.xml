        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="M:Microsoft.Build.Framework.IBuildEngine.BuildProjectFile(System.String,System.String[],System.Collections.IDictionary,System.Collections.IDictionary)">
          <source>Target outputs are returned as <ph id="ph1">&lt;xref:Microsoft.Build.Framework.ITaskItem&gt;</ph> arrays indexed by target name.</source>
          <target state="translated">目標輸出會當做傳回<ph id="ph1">&lt;xref:Microsoft.Build.Framework.ITaskItem&gt;</ph>由目標名稱的陣列編製索引。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Framework.IBuildEngine.ColumnNumberOfTaskNode">
          <source>Gets the line number of the task node within the project file that called it.</source>
          <target state="translated">取得呼叫工作之專案檔內工作節點的行號。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Framework.IBuildEngine.ColumnNumberOfTaskNode">
