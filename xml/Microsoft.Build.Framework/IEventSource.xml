    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Framework.IEventSource">
          <source>Defines the events raised by the build engine.</source>
          <target state="translated">定義建置 (Build) 引擎引發的事件。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Framework.IEventSource">
          <source>Loggers receive an object implementing this interface in their <ph id="ph1">&lt;see cref="M:Microsoft.Build.Framework.ILogger.Initialize(Microsoft.Build.Framework.IEventSource)" /&gt;</ph> method and use it to subscribe to the events they are interested in receiving.</source>
          <target state="translated">記錄器以 <ph id="ph1">&lt;see cref="M:Microsoft.Build.Framework.ILogger.Initialize(Microsoft.Build.Framework.IEventSource)" /&gt;</ph> 方法接收實作這個介面的物件，並使用它訂閱想要接收的事件。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.Build.Framework.IEventSource">
          <source>The following example shows how to write a basic logger that responds to build events.</source>
          <target state="translated">下列範例會示範如何撰寫基本記錄器回應建置事件。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.AnyEventRaised">
          <source>Occurs when a build raises any other type of build event.</source>
          <target state="translated">發生於建置引發任何其他型別的建置事件時。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.BuildFinished">
          <source>Occurs when a build finishes.</source>
          <target state="translated">發生於建置完成時。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.BuildStarted">
          <source>Occurs when a build starts.</source>
          <target state="translated">發生於建置開始時。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.CustomEventRaised">
          <source>Occurs when a build raises a custom event.</source>
          <target state="translated">發生於建置引發自訂事件時。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.ErrorRaised">
          <source>Occurs when a build raises an error.</source>
          <target state="translated">發生於建置引發錯誤時。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.MessageRaised">
          <source>Occurs when a build raises a message.</source>
          <target state="translated">發生於建置引發訊息時。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.ProjectFinished">
          <source>Occurs when a project finishes building.</source>
          <target state="translated">發生於專案完成建置時。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.ProjectStarted">
          <source>Occurs when a project starts building.</source>
          <target state="translated">發生於專案開始建置時。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.StatusEventRaised">
          <source>Occurs when a build raises a status event.</source>
          <target state="translated">發生於建置引發狀態事件時。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.TargetFinished">
          <source>Occurs when a target finishes building.</source>
          <target state="translated">發生於目標完成建置時。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.TargetStarted">
          <source>Occurs when a target starts building.</source>
          <target state="translated">發生於目標開始建置時。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.TaskFinished">
          <source>Occurs when a task completes execution.</source>
          <target state="translated">發生於工作完成執行時。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.TaskStarted">
          <source>Occurs when a task begins execution.</source>
          <target state="translated">發生於工作開始執行時。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="E:Microsoft.Build.Framework.IEventSource.WarningRaised">
          <source>Occurs when a build raises a warning.</source>
          <target state="translated">發生於建置引發警告時。</target>       </trans-unit>
