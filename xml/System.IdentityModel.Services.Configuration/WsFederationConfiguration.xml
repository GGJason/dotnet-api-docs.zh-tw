      <MemberSignature Language="C++ CLI" Value="public:&#xA; WsFederationConfiguration(System::IdentityModel::Services::Configuration::WSFederationElement ^ federationElement);" />
      <MemberType>Constructor</MemberType>
      <AssemblyInfo>
