      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessage">
          <source>Defines messages that occur during the <ph id="ph1">&lt;see cref="T:Microsoft.Build.Tasks.Deployment.Bootstrapper.BootstrapperBuilder" /&gt;</ph> build operation.</source>
          <target state="translated">定義會在 <ph id="ph1">&lt;see cref="T:Microsoft.Build.Tasks.Deployment.Bootstrapper.BootstrapperBuilder" /&gt;</ph> 建置作業期間出現的訊息。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessage.HelpId">
          <source>Gets the MSBuild Help ID for the host IDE.</source>
          <target state="translated">取得主機 IDE 的 MSBuild 說明識別碼。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessage.HelpId">
          <source>An integer value that represents the Help ID.</source>
          <target state="translated">表示說明識別碼的整數值。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessage.HelpKeyword">
          <source>Gets the MSBuild F1 Help keyword for the host IDE, or null.</source>
          <target state="translated">取得主機 IDE 的 MSBuild F1 Help 關鍵字，或為 null。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessage.HelpKeyword">
          <source>A string that represents the F1 Help keyword.</source>
          <target state="translated">表示 F1 Help 關鍵字的字串。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessage.Message">
          <source>Gets the details of the build message.</source>
          <target state="translated">取得建置訊息的詳細資料。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessage.Message">
          <source>A text string that describes the details of the build message.</source>
          <target state="translated">描述建置訊息詳細資料的文字字串。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessage.Severity">
          <source>Gets the severity of the build message.</source>
          <target state="translated">取得建置訊息的嚴重性。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessage.Severity">
          <source>The severity of the build message as defined by the <ph id="ph1">&lt;see cref="T:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessageSeverity" /&gt;</ph> class.</source>
          <target state="translated">建置訊息的嚴重性，如 <ph id="ph1">&lt;see cref="T:Microsoft.Build.Tasks.Deployment.Bootstrapper.BuildMessageSeverity" /&gt;</ph> 類別所定義。</target>       </trans-unit>
