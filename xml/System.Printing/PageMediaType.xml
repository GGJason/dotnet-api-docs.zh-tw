        <trans-unit id="119" translate="yes" xml:space="preserve" uid="F:System.Printing.PageMediaType.Fabric">
          <source>Fabric media.</source>
          <target state="translated">織物媒體。</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve" uid="F:System.Printing.PageMediaType.HighResolution">
          <source>Specialty high-resolution media.</source>
          <target state="translated">特殊高解析度媒體。</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve" uid="F:System.Printing.PageMediaType.Label">
          <source>Label media.</source>
          <target state="translated">標籤紙。</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve" uid="F:System.Printing.PageMediaType.MultiLayerForm">
          <source>Attached multipart forms.</source>
          <target state="translated">附加的複合型表單。</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve" uid="F:System.Printing.PageMediaType.MultiPartForm">
          <source>Individual multipart forms.</source>
          <target state="translated">個別複合型表單。</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve" uid="F:System.Printing.PageMediaType.None">
