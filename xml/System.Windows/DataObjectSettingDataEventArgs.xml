      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.DataObjectSettingDataEventArgs">
          <source>Contains arguments for the <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph>.<ph id="ph2">&lt;see cref="E:System.Windows.DataObject.SettingData" /&gt;</ph></source>
          <target state="translated">包含 <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph>.<ph id="ph2">&lt;see cref="E:System.Windows.DataObject.SettingData" /&gt;</ph></target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Windows.DataObjectSettingDataEventArgs">
          <source>event.</source>
          <target state="translated">事件的引數。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.DataObjectSettingDataEventArgs">
          <source>The <ph id="ph1">&lt;xref:System.Windows.DataObject.SettingData&gt;</ph> event occurs as part of a copy command or a drag command, once for each of the data formats to be added to the associated data object.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Windows.DataObject.SettingData&gt;</ph>事件發生的一部分複製或拖曳命令，一次每一個資料的格式，以便加入至相關聯的資料物件。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.DataObjectSettingDataEventArgs">
          <source>Typically, this event is connected to a content control such as <ph id="ph1">&lt;xref:System.Windows.Controls.TextBox&gt;</ph>.</source>
          <target state="translated">通常，此事件會連線至內容控制項例如<ph id="ph1">&lt;xref:System.Windows.Controls.TextBox&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Windows.DataObjectSettingDataEventArgs.#ctor(System.Windows.IDataObject,System.String)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph> to which a new data format is being added.</source>
          <target state="translated">新資料格式要加入的 <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Windows.DataObjectSettingDataEventArgs.#ctor(System.Windows.IDataObject,System.String)">
          <source>A string specifying the new data format that is being added to the accompanying data object.</source>
          <target state="translated">字串，指定要加入至隨附資料物件的新資料格式。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Windows.DataObjectSettingDataEventArgs.#ctor(System.Windows.IDataObject,System.String)">
          <source>See the <ph id="ph1">&lt;see cref="T:System.Windows.DataFormats" /&gt;</ph> class for a set of predefined data formats.</source>
          <target state="translated">如需一組預先定義的資料格式，請參閱 <ph id="ph1">&lt;see cref="T:System.Windows.DataFormats" /&gt;</ph> 類別。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Windows.DataObjectSettingDataEventArgs.#ctor(System.Windows.IDataObject,System.String)">
          <source>Initializes a new instance of <ph id="ph1">&lt;see cref="T:System.Windows.DataObjectSettingDataEventArgs" /&gt;</ph>.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.Windows.DataObjectSettingDataEventArgs" /&gt;</ph> 的新執行個體。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Windows.DataObjectSettingDataEventArgs.#ctor(System.Windows.IDataObject,System.String)">
          <source>Raised when <ph id="ph1">&lt;paramref name="dataObject" /&gt;</ph> or <ph id="ph2">&lt;paramref name="format" /&gt;</ph> is <bpt id="p1">**</bpt>null<ept id="p1">**</ept>.</source>
          <target state="translated">當 <ph id="ph1">&lt;paramref name="dataObject" /&gt;</ph> 或 <ph id="ph2">&lt;paramref name="format" /&gt;</ph> 為 <bpt id="p1">**</bpt>null<ept id="p1">**</ept> 時引發。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Windows.DataObjectSettingDataEventArgs.DataObject">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph> associated with the <ph id="ph2">&lt;see cref="E:System.Windows.DataObject.SettingData" /&gt;</ph> event.</source>
          <target state="translated">取得與 <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph> 事件相關聯的 <ph id="ph2">&lt;see cref="E:System.Windows.DataObject.SettingData" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.DataObjectSettingDataEventArgs.DataObject">
          <source>The <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph> associated with the <ph id="ph2">&lt;see cref="E:System.Windows.DataObject.SettingData" /&gt;</ph> event.</source>
          <target state="translated">與 <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph> 事件相關聯的 <ph id="ph2">&lt;see cref="E:System.Windows.DataObject.SettingData" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.Windows.DataObjectSettingDataEventArgs.Format">
          <source>Gets a string specifying the new data format that is being added to the accompanying data object.</source>
          <target state="translated">取得字串，這個字串指定要加入至隨附資料物件的新資料格式。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.DataObjectSettingDataEventArgs.Format">
          <source>A string specifying the new data format that is being added to the accompanying data object.</source>
          <target state="translated">字串，指定要加入至隨附資料物件的新資料格式。</target>       </trans-unit>
