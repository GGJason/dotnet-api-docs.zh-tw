      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.ScrollEventType">
          <source>Specifies the type of action used to raise the <ph id="ph1">&lt;see cref="E:System.Windows.Forms.ScrollBar.Scroll" /&gt;</ph> event.</source>
          <target state="translated">指定用來引發 <ph id="ph1">&lt;see cref="E:System.Windows.Forms.ScrollBar.Scroll" /&gt;</ph> 事件的動作的型別。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.ScrollEventType">
          <source>This enumeration is used by <ph id="ph1">&lt;xref:System.Windows.Forms.ScrollEventArgs.Type%2A?displayProperty=nameWithType&gt;</ph>.</source>
          <target state="translated">這個列舉型別由<ph id="ph1">&lt;xref:System.Windows.Forms.ScrollEventArgs.Type%2A?displayProperty=nameWithType&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.EndScroll">
          <source>The scroll box has stopped moving.</source>
          <target state="translated">捲動方塊已經停止移動。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.First">
          <source>The scroll box was moved to the <ph id="ph1">&lt;see cref="P:System.Windows.Forms.ScrollBar.Minimum" /&gt;</ph> position.</source>
          <target state="translated">捲動方塊已移至 <ph id="ph1">&lt;see cref="P:System.Windows.Forms.ScrollBar.Minimum" /&gt;</ph> 位置。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.LargeDecrement">
          <source>The scroll box moved a large distance.</source>
          <target state="translated">捲動方塊移動一大段距離。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.LargeDecrement">
          <source>The user clicked the scroll bar to the left(horizontal) or above(vertical) the scroll box, or pressed the PAGE UP key.</source>
          <target state="translated">使用者按一下捲軸上捲動方塊的左方 (水平) 或上方 (垂直)，或是按 PAGE UP 鍵。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.LargeIncrement">
          <source>The scroll box moved a large distance.</source>
          <target state="translated">捲動方塊移動一大段距離。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.LargeIncrement">
          <source>The user clicked the scroll bar to the right(horizontal) or below(vertical) the scroll box, or pressed the PAGE DOWN key.</source>
          <target state="translated">使用者按一下捲軸上捲動方塊的右方 (水平) 或下方 (垂直)，或是按 PAGE DOWN 鍵。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.Last">
          <source>The scroll box was moved to the <ph id="ph1">&lt;see cref="P:System.Windows.Forms.ScrollBar.Maximum" /&gt;</ph> position.</source>
          <target state="translated">捲動方塊已移至 <ph id="ph1">&lt;see cref="P:System.Windows.Forms.ScrollBar.Maximum" /&gt;</ph> 位置。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.SmallDecrement">
          <source>The scroll box was moved a small distance.</source>
          <target state="translated">捲動方塊移動一小段距離。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.SmallDecrement">
          <source>The user clicked the left(horizontal) or top(vertical) scroll arrow, or pressed the UP ARROW key.</source>
          <target state="translated">使用者按一下向左 (水平) 或向上 (垂直) 捲動箭號，或者是按向上鍵。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.SmallIncrement">
          <source>The scroll box was moved a small distance.</source>
          <target state="translated">捲動方塊移動一小段距離。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.SmallIncrement">
          <source>The user clicked the right(horizontal) or bottom(vertical) scroll arrow, or pressed the DOWN ARROW key.</source>
          <target state="translated">使用者按一下向右 (水平) 或向下 (垂直) 捲動箭號，或按向下鍵。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.ThumbPosition">
          <source>The scroll box was moved.</source>
          <target state="translated">移動捲動方塊。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ScrollEventType.ThumbTrack">
          <source>The scroll box is currently being moved.</source>
          <target state="translated">目前正在移動捲動方塊。</target>       </trans-unit>
