        <trans-unit id="101" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Activities.ActivityAction`2">
          <source>The type of the first in argument of the activity delegate.</source>
          <target state="translated">活動委派的第一個 in 引數的型別。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Activities.ActivityAction`2">
