      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.LocationReferenceEnvironment">
          <source>Provides a representation of the environment of variables and arguments that is available to an activity at runtime.</source>
          <target state="translated">提供活動執行階段中可用之變數與引數的環境表示。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.#ctor">
          <source>Called by derived class constructors to initialize the object.</source>
          <target state="translated">由衍生類別的建構函式呼叫，以初始化物件。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.GetLocationReferences">
          <source>When overridden in a derived class, returns a collection of <ph id="ph1">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph> objects in the current scope.</source>
          <target state="translated">在衍生類別中覆寫時，傳回現有範圍中 <ph id="ph1">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph> 物件的集合。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.GetLocationReferences">
          <source>A collection of location reference objects in the current scope.</source>
          <target state="translated">現有範圍中位置參考物件的集合。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.IsVisible(System.Activities.LocationReference)">
          <source>The location to evaluate.</source>
          <target state="translated">要評估的位置。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.IsVisible(System.Activities.LocationReference)">
          <source>When overridden in a derived class, determines whether the specified <ph id="ph1">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph> is visible in the current scope.</source>
          <target state="translated">在衍生類別中覆寫時，判斷在現有範圍中是否會顯示指定的 <ph id="ph1">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.IsVisible(System.Activities.LocationReference)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the <ph id="ph2">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph> is visible in the current scope; otherwise, <ph id="ph3">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果在目前範圍中會顯示 <ph id="ph2">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph>，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph3">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Activities.LocationReferenceEnvironment.Parent">
          <source>Gets or sets the containing <ph id="ph1">&lt;see cref="T:System.Activities.LocationReferenceEnvironment" /&gt;</ph>.</source>
          <target state="translated">取得或設定包含的 <ph id="ph1">&lt;see cref="T:System.Activities.LocationReferenceEnvironment" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.LocationReferenceEnvironment.Parent">
          <source>The containing <ph id="ph1">&lt;see cref="T:System.Activities.LocationReferenceEnvironment" /&gt;</ph>.</source>
          <target state="translated">包含的 <ph id="ph1">&lt;see cref="T:System.Activities.LocationReferenceEnvironment" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Activities.LocationReferenceEnvironment.Root">
          <source>Gets the activity that determines the scoping rules for this environment.</source>
          <target state="translated">取得判斷此環境之範圍規則的活動。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.LocationReferenceEnvironment.Root">
          <source>The activity that determines the scoping rules for this environment.</source>
          <target state="translated">判斷此環境之範圍規則的活動。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.TryGetLocationReference(System.String,System.Activities.LocationReference@)">
          <source>The name of the <ph id="ph1">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph> to retrieve.</source>
          <target state="translated">要擷取之 <ph id="ph1">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph> 的名稱。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.TryGetLocationReference(System.String,System.Activities.LocationReference@)">
          <source>When this method returns <ph id="ph1">&lt;see langword="true" /&gt;</ph>, <bpt id="p1">&lt;c&gt;</bpt>result<ept id="p1">&lt;/c&gt;</ept> contains the <ph id="ph2">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph> with the specified name; otherwise, <ph id="ph3">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated">當此方法傳回 <ph id="ph1">&lt;see langword="true" /&gt;</ph> 時，<bpt id="p1">&lt;c&gt;</bpt>result<ept id="p1">&lt;/c&gt;</ept> 會包含帶有指定名稱的 <ph id="ph2">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph>；否則為 <ph id="ph3">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.TryGetLocationReference(System.String,System.Activities.LocationReference@)">
          <source>This parameter is passed uninitialized.</source>
          <target state="translated">這個參數會以未初始化的狀態傳遞。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.TryGetLocationReference(System.String,System.Activities.LocationReference@)">
          <source>When overridden in a derived class, attempts to get the specified <ph id="ph1">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph>.</source>
          <target state="translated">在衍生類別中覆寫時，嘗試取得指定的 <ph id="ph1">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.Activities.LocationReferenceEnvironment.TryGetLocationReference(System.String,System.Activities.LocationReference@)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if a <ph id="ph2">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph> with the specified name is present; otherwise, <ph id="ph3">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果出現具有指定的名稱的 <ph id="ph2">&lt;see cref="T:System.Activities.LocationReference" /&gt;</ph>，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph3">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
