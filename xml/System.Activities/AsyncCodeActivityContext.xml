      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.AsyncCodeActivityContext">
          <source>The execution context for an asynchronous activity.</source>
          <target state="translated">非同步活動的執行內容。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Activities.AsyncCodeActivityContext">
          <source>This is a sealed class and is derived from <ph id="ph1">&lt;xref:System.Activities.CodeActivityContext&gt;</ph>.</source>
          <target state="translated">這是密封類別，衍生自 <ph id="ph1">&lt;xref:System.Activities.CodeActivityContext&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Activities.AsyncCodeActivityContext.IsCancellationRequested">
          <source>Gets a value that indicates whether the currently executing <ph id="ph1">&lt;see cref="T:System.Activities.AsyncCodeActivity" /&gt;</ph> is to be canceled.</source>
          <target state="translated">取得值，這個值會指出是否要取消目前執行中的 <ph id="ph1">&lt;see cref="T:System.Activities.AsyncCodeActivity" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.AsyncCodeActivityContext.IsCancellationRequested">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the currently executing <ph id="ph2">&lt;see cref="T:System.Activities.AsyncCodeActivity" /&gt;</ph> is to be canceled; otherwise, <ph id="ph3">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果要取消目前執行中的 <ph id="ph2">&lt;see cref="T:System.Activities.AsyncCodeActivity" /&gt;</ph>，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph3">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.AsyncCodeActivityContext.MarkCanceled">
          <source>Marks the current activity instance as canceled.</source>
          <target state="translated">將目前活動的執行個體標記為取消。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Activities.AsyncCodeActivityContext.UserState">
          <source>Gets or sets a user-defined object associated with the current asynchronous activity.</source>
          <target state="translated">取得或設定與目前非同步活動相關聯的使用者定義物件。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.AsyncCodeActivityContext.UserState">
          <source>The user-defined object.</source>
          <target state="translated">使用者定義的物件。</target>       </trans-unit>
