      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.Services.Configuration.WebServiceProtocols">
          <source>Specifies the transmission protocols that are used to decrypt data sent from a client browser in the HTTP request.</source>
          <target state="translated">指定傳輸通訊協定，用來解密在 HTTP 要求中從用戶端瀏覽器傳送而來的資料。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.Services.Configuration.WebServiceProtocols">
          <source>Using the HttpPostLocalhost value of this enumeration does not prevent access to the test or web service from remote computers.</source>
          <target state="translated">使用這個列舉型別的 HttpPostLocalhost 值不會阻止存取到測試或 web 服務的遠端電腦。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.Services.Configuration.WebServiceProtocols">
          <source>This value checks only the host header of the request; no other validation, authentication, or authorization checks are performed.</source>
          <target state="translated">這個值會檢查只要求; 的主機標頭會不執行任何其他驗證、 驗證或授權檢查。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.Services.Configuration.WebServiceProtocols">
          <source>This check mitigates only browser-based redirection attacks.</source>
          <target state="translated">這項檢查會降低只瀏覽器為基礎的重新導向攻擊。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.Services.Configuration.WebServiceProtocols">
          <source>Authentication and authorization are still required to restrict access to a Web service.</source>
          <target state="translated">驗證和授權仍然需要，來限制存取 Web 服務。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Configuration.WebServiceProtocols.AnyHttpSoap">
          <source>Any version of the HTTP SOAP protocol.</source>
          <target state="translated">任何版本的 HTTP SOAP 通訊協定。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Configuration.WebServiceProtocols.Documentation">
          <source>The Web Services Documentation protocol.</source>
          <target state="translated">Web Services Documentation 通訊協定。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Configuration.WebServiceProtocols.HttpGet">
          <source>The HTTP GET protocol.</source>
          <target state="translated">HTTP GET 通訊協定。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Configuration.WebServiceProtocols.HttpPost">
          <source>The HTTP POST protocol.</source>
          <target state="translated">HTTP POST 通訊協定。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Configuration.WebServiceProtocols.HttpPostLocalhost">
          <source>The HTTP POST LOCALHOST protocol.</source>
          <target state="translated">HTTP POST LOCALHOST 通訊協定。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.Web.Services.Configuration.WebServiceProtocols.HttpPostLocalhost">
          <source>See the Remarks section.</source>
          <target state="translated">請參閱＜備註＞一節。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.Web.Services.Configuration.WebServiceProtocols.HttpPostLocalhost">
          <source>This value only checks the request's host header to mitigate only browser-redirect attacks.</source>
          <target state="translated">這個值只會檢查要求的主機標頭，以減輕只瀏覽器重新導向攻擊。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.Web.Services.Configuration.WebServiceProtocols.HttpPostLocalhost">
          <source>Authentication and authorization are still required to restrict access to a Web service.</source>
          <target state="translated">驗證和授權仍然需要，來限制存取 Web 服務。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Configuration.WebServiceProtocols.HttpSoap">
          <source>The HTTP SOAP protocol.</source>
          <target state="translated">HTTP SOAP 通訊協定。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Configuration.WebServiceProtocols.HttpSoap12">
          <source>The HTTP SOAP version 1.2 protocol.</source>
          <target state="translated">HTTP SOAP 1.2 版通訊協定。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Configuration.WebServiceProtocols.Unknown">
          <source>Unknown protocol.</source>
          <target state="translated">未知的通訊協定。</target>       </trans-unit>
