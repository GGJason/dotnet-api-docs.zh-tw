        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Controls.Primitives.ItemsChangedEventArgs">
          <source>Provides data for the <ph id="ph1">&lt;see cref="E:System.Windows.Controls.ItemContainerGenerator.ItemsChanged" /&gt;</ph> event.</source>
          <target state="translated">提供 <ph id="ph1">&lt;see cref="E:System.Windows.Controls.ItemContainerGenerator.ItemsChanged" /&gt;</ph> 事件的資料。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Controls.Primitives.ItemsChangedEventArgs">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Controls.ItemContainerGenerator.ItemsChanged&gt;</ph> event is raised by an <ph id="ph2">&lt;xref:System.Windows.Controls.Primitives.IItemContainerGenerator&gt;</ph> to inform layouts that the items collection has changed.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Windows.Controls.ItemContainerGenerator.ItemsChanged&gt;</ph>就會引發事件<ph id="ph2">&lt;xref:System.Windows.Controls.Primitives.IItemContainerGenerator&gt;</ph>通知已變更項目集合的配置。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Windows.Controls.Primitives.ItemsChangedEventArgs.Action">
          <source>Gets the action that occurred on the items collection.</source>
          <target state="translated">取得在項目集合上發生的動作。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Controls.Primitives.ItemsChangedEventArgs.Action">
          <source>Returns the action that occurred.</source>
          <target state="translated">傳回發生的動作。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.Windows.Controls.Primitives.ItemsChangedEventArgs.ItemCount">
          <source>Gets the number of items that were involved in the change.</source>
          <target state="translated">取得變更所涉及項目的數目。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Controls.Primitives.ItemsChangedEventArgs.ItemCount">
          <source>Integer that represents the number of items involved in the change.</source>
