|<xref:System.Windows.Controls.Primitives.TickBar.Minimum%2A?displayProperty=nameWithType>|<xref:System.Windows.Controls.Primitives.RangeBase.Minimum%2A?displayProperty=nameWithType>|  
|<xref:System.Windows.Controls.Primitives.TickBar.SelectionEnd%2A?displayProperty=nameWithType>|<xref:System.Windows.Controls.Slider.SelectionEnd%2A?displayProperty=nameWithType>|  
|<xref:System.Windows.Controls.Primitives.TickBar.SelectionStart%2A?displayProperty=nameWithType>|<xref:System.Windows.Controls.Slider.SelectionStart%2A?displayProperty=nameWithType>|  
|<xref:System.Windows.Controls.Primitives.TickBar.TickFrequency%2A?displayProperty=nameWithType>|<xref:System.Windows.Controls.Slider.TickFrequency%2A?displayProperty=nameWithType>|  
|<xref:System.Windows.Controls.Primitives.TickBar.Ticks%2A?displayProperty=nameWithType>|<xref:System.Windows.Controls.Slider.Ticks%2A?displayProperty=nameWithType>|  
  
 ]]></format>
    </remarks>
