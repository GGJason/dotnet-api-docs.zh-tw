      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation">
          <source>Specifies the type of location for the style sheet for a mobile control or page.</source>
          <target state="translated">指定行動控制項或頁面的樣式表位置類型。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation">
          <source>For information about how to develop ASP.NET mobile applications, see <bpt id="p1">&lt;see href="https://docs.microsoft.com/aspnet/mobile/overview"&gt;</bpt>Mobile Apps <ph id="ph1">&amp;amp;</ph> Sites with ASP.NET<ept id="p1">&lt;/see&gt;</ept>.</source>
          <target state="translated">如需如何開發 ASP.NET 行動應用程式的資訊，請參閱<bpt id="p1">&lt;see href="https://docs.microsoft.com/aspnet/mobile/overview"&gt;</bpt>使用 ASP.NET 來開發行動應用程式與網站<ept id="p1">&lt;/see&gt;</ept> (機器翻譯)。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation">
          <source>The <ph id="ph1">&lt;xref:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation&gt;</ph> enumeration is used by classes in the <ph id="ph2">&lt;xref:System.Web.UI.MobileControls.Adapters.XhtmlAdapters&gt;</ph> namespace to facilitate creating XHTML markup.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation&gt;</ph>中的類別會使用列舉<ph id="ph2">&lt;xref:System.Web.UI.MobileControls.Adapters.XhtmlAdapters&gt;</ph>命名空間，以便建立 XHTML 標記。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation.ApplicationCache">
          <source>The style sheet is stored in the Application Cache.</source>
          <target state="translated">樣式表會儲存在應用程式快取中。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation.Internal">
          <source>The style sheet is a set of styles within the current source file (either .aspx or .ascx).</source>
          <target state="translated">樣式表是目前來源檔 (.aspx 或 .ascx) 中的樣式集。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation.None">
          <source>Not currently used.</source>
          <target state="translated">目前無法使用。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation.NotSet">
          <source>Default value.</source>
          <target state="translated">預設值。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation.NotSet">
          <source>The location has not yet been determined.</source>
          <target state="translated">尚未決定位置。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation.PhysicalFile">
          <source>The style sheet is an external physical file, such as MyStylesheet.css.</source>
          <target state="translated">樣式表是外部實體檔案，例如 MyStylesheet.css。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Web.UI.MobileControls.Adapters.XhtmlAdapters.StyleSheetLocation.SessionState">
          <source>The style sheet is stored in the current Session state.</source>
          <target state="translated">樣式表會儲存在目前的工作階段 (Session) 狀態中。</target>       </trans-unit>
