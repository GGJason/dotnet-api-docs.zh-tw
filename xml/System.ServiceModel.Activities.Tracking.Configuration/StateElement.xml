        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Activities.Tracking.Configuration.StateElement">
          <source>A configuration element that specifies workflow instance state or activity instance state the user subscribes tracking information for.</source>
          <target state="translated">組態項目，這個項目會指定工作流程執行個體狀態或活動執行個體狀態，而這些狀態則是使用者訂閱追蹤資訊的目的。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceModel.Activities.Tracking.Configuration.StateElement">
          <source>This element is placed within a <ph id="ph1">&lt;xref:System.ServiceModel.Activities.Tracking.Configuration.StateElementCollection&gt;</ph> within a <ph id="ph2">&lt;xref:System.ServiceModel.Activities.Tracking.Configuration.ActivityStateQueryElement&gt;</ph> to specify the activity states the query should filter on.</source>
          <target state="translated">這個項目位於 <ph id="ph1">&lt;xref:System.ServiceModel.Activities.Tracking.Configuration.StateElementCollection&gt;</ph> 中的 <ph id="ph2">&lt;xref:System.ServiceModel.Activities.Tracking.Configuration.ActivityStateQueryElement&gt;</ph> 內，會用於指定要做為查詢篩選條件的活動狀態。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Activities.Tracking.Configuration.StateElement.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Activities.Tracking.Configuration.StateElement" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.ServiceModel.Activities.Tracking.Configuration.StateElement" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Activities.Tracking.Configuration.StateElement.ElementKey">
          <source>Gets a key that uniquely identifies the element query.</source>
          <target state="translated">取得可唯一識別項目查詢的索引鍵。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Activities.Tracking.Configuration.StateElement.ElementKey">
          <source>The element key.</source>
          <target state="translated">項目索引鍵。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Activities.Tracking.Configuration.StateElement.Name">
          <source>Gets or sets the state name to filter on.</source>
          <target state="translated">取得或設定要做為篩選條件的狀態名稱。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Activities.Tracking.Configuration.StateElement.Name">
          <source>The state name to filter on.</source>
          <target state="translated">做為篩選條件的狀態名稱。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Activities.Tracking.Configuration.StateElement.Name">
          <source>The default value is "*" (all).</source>
          <target state="translated">預設值為 "*" (全部)。</target>       </trans-unit>
