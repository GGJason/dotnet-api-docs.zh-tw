      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.Hosting.AspNetMemoryMonitor">
          <source>ASP.NET default implementation of memory monitor.</source>
          <target state="translated">記憶體監視器的 ASP.NET 預設實作。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Web.Hosting.AspNetMemoryMonitor.DefaultLowPhysicalMemoryObserver">
          <source>Gets or sets the default low-memory observer.</source>
          <target state="translated">取得或設定預設記憶體不足的觀察者。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Web.Hosting.AspNetMemoryMonitor.DefaultLowPhysicalMemoryObserver">
          <source>Setting this property unsubscribes the 'default' observer that was there before.</source>
          <target state="translated">設定此屬性會取消訂閱之前已存在的 'default' 觀察者。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.Web.Hosting.AspNetMemoryMonitor.DefaultLowPhysicalMemoryObserver">
          <source>There can only be one 'default.'</source>
          <target state="translated">只能有一個 'default'。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.Web.Hosting.AspNetMemoryMonitor.DefaultLowPhysicalMemoryObserver">
          <source>To provide additional observers, use the <ph id="ph1">&lt;see cref="Overload:System.Web.Hosting.AspNetMemoryMonitor.Subscribe" /&gt;</ph> method.</source>
          <target state="translated">若要提供額外的觀察者，請使用 <ph id="ph1">&lt;see cref="Overload:System.Web.Hosting.AspNetMemoryMonitor.Subscribe" /&gt;</ph> 方法。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Web.Hosting.AspNetMemoryMonitor.DefaultRecycleLimitObserver">
          <source>Gets or sets the default recycle limit observer.</source>
          <target state="translated">取得或設定預設的回收限制觀察者。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Web.Hosting.AspNetMemoryMonitor.DefaultRecycleLimitObserver">
          <source>Setting this property unsubscribes the 'default' observer that was there before.</source>
          <target state="translated">設定此屬性會取消訂閱之前已存在的 'default' 觀察者。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Web.Hosting.AspNetMemoryMonitor.DefaultRecycleLimitObserver">
          <source>There can only be one 'default.'</source>
          <target state="translated">只能有一個 'default'。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.Web.Hosting.AspNetMemoryMonitor.DefaultRecycleLimitObserver">
          <source>To provide additional observers, use the <ph id="ph1">&lt;see cref="Overload:System.Web.Hosting.AspNetMemoryMonitor.Subscribe" /&gt;</ph> method.</source>
          <target state="translated">若要提供額外的觀察者，請使用 <ph id="ph1">&lt;see cref="Overload:System.Web.Hosting.AspNetMemoryMonitor.Subscribe" /&gt;</ph> 方法。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Web.Hosting.AspNetMemoryMonitor.Dispose">
          <source>Releases all resources used by the memory monitor.</source>
          <target state="translated">釋放記憶體監視器所使用的所有資源。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Web.Hosting.AspNetMemoryMonitor.Start">
          <source>Start monitoring and firing notifications.</source>
          <target state="translated">開始監視和引發通知。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Web.Hosting.AspNetMemoryMonitor.Stop">
          <source>Stop monitoring and firing notifications.</source>
          <target state="translated">停止監視和引發通知。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="T:System.Web.Hosting.AspNetMemoryMonitor">
          <source>Subscribes an observer in addition to the default observer.</source>
          <target state="translated">訂閱除了預設觀察者以外的觀察器。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Web.Hosting.AspNetMemoryMonitor.Subscribe(System.IObserver{System.Web.Hosting.LowPhysicalMemoryInfo})">
