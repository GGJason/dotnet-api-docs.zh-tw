      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Diagnostics.PerformanceCounterPermissionAttribute">
          <source>Allows declaritive performance counter permission checks.</source>
          <target state="translated">允許宣告性效能計數器使用權限檢查。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.PerformanceCounterPermissionAttribute">
          <source>For more information about using attributes, see <bpt id="p1">[</bpt>Attributes<ept id="p1">](~/docs/standard/attributes/index.md)</ept>.</source>
          <target state="translated">如需屬性用法的詳細資訊，請參閱<bpt id="p1">[</bpt>屬性<ept id="p1">](~/docs/standard/attributes/index.md)</ept>。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Diagnostics.PerformanceCounterPermissionAttribute.#ctor(System.Security.Permissions.SecurityAction)">
          <source>One of the <ph id="ph1">&lt;see cref="T:System.Security.Permissions.SecurityAction" /&gt;</ph> values.</source>
          <target state="translated">其中一個 <ph id="ph1">&lt;see cref="T:System.Security.Permissions.SecurityAction" /&gt;</ph> 值。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Diagnostics.PerformanceCounterPermissionAttribute.#ctor(System.Security.Permissions.SecurityAction)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Diagnostics.PerformanceCounterPermissionAttribute" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.Diagnostics.PerformanceCounterPermissionAttribute" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.Diagnostics.PerformanceCounterPermissionAttribute.CategoryName">
          <source>Gets or sets the name of the performance counter category.</source>
          <target state="translated">取得或設定效能計數器分類的名稱。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Diagnostics.PerformanceCounterPermissionAttribute.CategoryName">
          <source>The name of the performance counter category (performance object).</source>
          <target state="translated">效能計數器分類 (效能物件) 的名稱。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Diagnostics.PerformanceCounterPermissionAttribute.CategoryName">
          <source>The value is <ph id="ph1">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated">值為 <ph id="ph1">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Diagnostics.PerformanceCounterPermissionAttribute.CreatePermission">
          <source>Creates the permission based on the requested access levels that are set through the <ph id="ph1">&lt;see cref="P:System.Diagnostics.PerformanceCounterPermissionAttribute.PermissionAccess" /&gt;</ph> property on the attribute.</source>
          <target state="translated">根據所要求的存取層級建立使用權限，該層級透過屬性 (Attribute) 上的 <ph id="ph1">&lt;see cref="P:System.Diagnostics.PerformanceCounterPermissionAttribute.PermissionAccess" /&gt;</ph> 屬性 (Property) 來設定。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Diagnostics.PerformanceCounterPermissionAttribute.CreatePermission">
          <source>An <ph id="ph1">&lt;see cref="T:System.Security.IPermission" /&gt;</ph> that represents the created permission.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Security.IPermission" /&gt;</ph>，表示建立的使用權限。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Diagnostics.PerformanceCounterPermissionAttribute.MachineName">
          <source>Gets or sets the computer name for the performance counter.</source>
          <target state="translated">取得或設定效能計數器的電腦名稱。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Diagnostics.PerformanceCounterPermissionAttribute.MachineName">
          <source>The server on which the category of the performance counter resides.</source>
          <target state="translated">效能計數器的分類所駐留的伺服器。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.Diagnostics.PerformanceCounterPermissionAttribute.MachineName">
          <source>The <ph id="ph1">&lt;see cref="P:System.Diagnostics.PerformanceCounterPermissionAttribute.MachineName" /&gt;</ph> format is invalid.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="P:System.Diagnostics.PerformanceCounterPermissionAttribute.MachineName" /&gt;</ph> 格式無效。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="P:System.Diagnostics.PerformanceCounterPermissionAttribute.PermissionAccess">
          <source>Gets or sets the access levels used in the permissions request.</source>
          <target state="translated">取得或設定用於使用權限要求的存取等級。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Diagnostics.PerformanceCounterPermissionAttribute.PermissionAccess">
          <source>A bitwise combination of the <ph id="ph1">&lt;see cref="T:System.Diagnostics.PerformanceCounterPermissionAccess" /&gt;</ph> values.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Diagnostics.PerformanceCounterPermissionAccess" /&gt;</ph> 值的位元組合。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Diagnostics.PerformanceCounterPermissionAttribute.PermissionAccess">
          <source>The default is <ph id="ph1">&lt;see cref="F:System.Diagnostics.EventLogPermissionAccess.Write" /&gt;</ph>.</source>
          <target state="translated">預設值為 <ph id="ph1">&lt;see cref="F:System.Diagnostics.EventLogPermissionAccess.Write" /&gt;</ph>。</target>       </trans-unit>
