      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Automation.Peers.ButtonBaseAutomationPeer">
          <source>Represents a base class for exposing elements derived from <ph id="ph1">&lt;see cref="T:System.Windows.Controls.Primitives.ButtonBase" /&gt;</ph> to UI Automation.</source>
          <target state="translated">代表用來將衍生自 <ph id="ph1">&lt;see cref="T:System.Windows.Controls.Primitives.ButtonBase" /&gt;</ph> 的元素公開至使用者介面自動化的基底類別。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.#ctor(System.Windows.Controls.Primitives.ButtonBase)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Windows.Controls.Primitives.ButtonBase" /&gt;</ph> that is associated with this peer.</source>
          <target state="translated">與這個對等個體相關聯的 <ph id="ph1">&lt;see cref="T:System.Windows.Controls.Primitives.ButtonBase" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.#ctor(System.Windows.Controls.Primitives.ButtonBase)">
          <source>Provides initialization for base class values when called by the constructor of a derived class.</source>
          <target state="translated">在透過衍生類別的建構函式呼叫時，提供基本類別值的初始設定。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetAcceleratorKeyCore">
          <source>Gets the accelerator key for the element associated with this <ph id="ph1">&lt;see cref="T:System.Windows.Automation.Peers.ButtonBaseAutomationPeer" /&gt;</ph>.</source>
          <target state="translated">取得與這個 <ph id="ph1">&lt;see cref="T:System.Windows.Automation.Peers.ButtonBaseAutomationPeer" /&gt;</ph> 相關聯之元素的快速鍵。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetAcceleratorKeyCore">
          <source>Called by <ph id="ph1">&lt;see cref="M:System.Windows.Automation.Peers.AutomationPeer.GetAcceleratorKey" /&gt;</ph>.</source>
          <target state="translated">由 <ph id="ph1">&lt;see cref="M:System.Windows.Automation.Peers.AutomationPeer.GetAcceleratorKey" /&gt;</ph> 呼叫。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetAcceleratorKeyCore">
          <source>A string containing the accelerator key.</source>
          <target state="translated">包含快速鍵的字串。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetAutomationIdCore">
          <source>Gets the <ph id="ph1">&lt;see cref="P:System.Windows.Automation.AutomationProperties.AutomationId" /&gt;</ph> for the element associated with this <ph id="ph2">&lt;see cref="T:System.Windows.Automation.Peers.ButtonBaseAutomationPeer" /&gt;</ph>.</source>
          <target state="translated">取得與這個 <ph id="ph1">&lt;see cref="P:System.Windows.Automation.AutomationProperties.AutomationId" /&gt;</ph> 相關聯的 <ph id="ph2">&lt;see cref="T:System.Windows.Automation.Peers.ButtonBaseAutomationPeer" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetAutomationIdCore">
          <source>Called by <ph id="ph1">&lt;see cref="M:System.Windows.Automation.Peers.AutomationPeer.GetAutomationId" /&gt;</ph>.</source>
          <target state="translated">由 <ph id="ph1">&lt;see cref="M:System.Windows.Automation.Peers.AutomationPeer.GetAutomationId" /&gt;</ph> 呼叫。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetAutomationIdCore">
          <source>The string that contains the <ph id="ph1">&lt;see cref="P:System.Windows.Automation.AutomationProperties.AutomationId" /&gt;</ph>.</source>
          <target state="translated">包含 <ph id="ph1">&lt;see cref="P:System.Windows.Automation.AutomationProperties.AutomationId" /&gt;</ph> 的字串。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetAutomationIdCore">
          <source>The value of the automation identifier is the <ph id="ph1">&lt;xref:System.Windows.Automation.AutomationProperties.AutomationId%2A&gt;</ph> property, and if that isn't set, it's the <ph id="ph2">&lt;xref:System.Windows.FrameworkElement.Name%2A&gt;</ph> property.</source>
          <target state="translated">自動化識別項的值是<ph id="ph1">&lt;xref:System.Windows.Automation.AutomationProperties.AutomationId%2A&gt;</ph>屬性，以及如果您未設定的它是<ph id="ph2">&lt;xref:System.Windows.FrameworkElement.Name%2A&gt;</ph>屬性。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetNameCore">
          <source>Gets the name of the class of the element associated with this <ph id="ph1">&lt;see cref="T:System.Windows.Automation.Peers.ButtonBaseAutomationPeer" /&gt;</ph>.</source>
          <target state="translated">取得與這個 <ph id="ph1">&lt;see cref="T:System.Windows.Automation.Peers.ButtonBaseAutomationPeer" /&gt;</ph> 相關聯之項目的類別名稱。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetNameCore">
          <source>Called by <ph id="ph1">&lt;see cref="M:System.Windows.Automation.Peers.AutomationPeer.GetName" /&gt;</ph>.</source>
          <target state="translated">由 <ph id="ph1">&lt;see cref="M:System.Windows.Automation.Peers.AutomationPeer.GetName" /&gt;</ph> 呼叫。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Windows.Automation.Peers.ButtonBaseAutomationPeer.GetNameCore">
          <source>A string that contains the class name, minus the accelerator key.</source>
          <target state="translated">包含類別名稱 (不含快速鍵) 的字串。</target>       </trans-unit>
