      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer">
          <source>Represents a class that provides the CodeDOM serialization services to an activity at design time.</source>
          <target state="translated">表示在設計階段提供 CodeDOM 序列化 (Serialization) 服務給活動的類別。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer">
          <source><ph id="ph1">&lt;xref:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer&gt;</ph> controls the generation of component initialization code for the <ph id="ph2">&lt;xref:System.Workflow.ComponentModel.Activity&gt;</ph> class at design time.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer&gt;</ph> 會在設計階段針對 <ph id="ph2">&lt;xref:System.Workflow.ComponentModel.Activity&gt;</ph> 類別控制元件初始化程式碼的產生。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer.MarkupFileNameProperty">
          <source>The name of the markup file to write to.</source>
          <target state="translated">要寫入的標記檔案名稱。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer.Serialize(System.ComponentModel.Design.Serialization.IDesignerSerializationManager,System.Object)">
          <source>The serialization manager to use during the serialization process.</source>
          <target state="translated">要在序列化期間使用的序列化管理員。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer.Serialize(System.ComponentModel.Design.Serialization.IDesignerSerializationManager,System.Object)">
          <source>An <ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph> that derives from <ph id="ph2">&lt;see cref="T:System.Workflow.ComponentModel.Activity" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph>，衍生自 <ph id="ph2">&lt;see cref="T:System.Workflow.ComponentModel.Activity" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer.Serialize(System.ComponentModel.Design.Serialization.IDesignerSerializationManager,System.Object)">
          <source>Serializes the specified object into a CodeDOM object.</source>
          <target state="translated">將指定的物件序列化為 CodeDOM 物件。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer.Serialize(System.ComponentModel.Design.Serialization.IDesignerSerializationManager,System.Object)">
          <source>A CodeDOM object representing the object that has been serialized.</source>
          <target state="translated">CodeDOM 物件，表示已序列化的物件。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer.Serialize(System.ComponentModel.Design.Serialization.IDesignerSerializationManager,System.Object)">
          <source>Serialize returns a null reference (<ph id="ph1">`Nothing`</ph>) if the activity it attempts to serialize is the child activity of a custom composite activity.</source>
          <target state="translated">如果嘗試要序列化的活動是自訂複合活動的子活動，則序列化會傳回 null 參考 (<ph id="ph1">`Nothing`</ph>)。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer.Serialize(System.ComponentModel.Design.Serialization.IDesignerSerializationManager,System.Object)">
          <source><ph id="ph1">&lt;paramref name="manager" /&gt;</ph> or <ph id="ph2">&lt;paramref name="obj" /&gt;</ph> contains a null reference (<ph id="ph3">&lt;see langword="Nothing" /&gt;</ph> in Visual Basic).</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="manager" /&gt;</ph> 或 <ph id="ph2">&lt;paramref name="obj" /&gt;</ph> 包含 null 參考 (在 Visual Basic 中為 <ph id="ph3">&lt;see langword="Nothing" /&gt;</ph>)。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Workflow.ComponentModel.Serialization.ActivityCodeDomSerializer.Serialize(System.ComponentModel.Design.Serialization.IDesignerSerializationManager,System.Object)">
          <source><ph id="ph1">&lt;paramref name="obj" /&gt;</ph> cannot be converted to an <ph id="ph2">&lt;see cref="T:System.Workflow.ComponentModel.Activity" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="obj" /&gt;</ph> 無法轉換成 <ph id="ph2">&lt;see cref="T:System.Workflow.ComponentModel.Activity" /&gt;</ph>。</target>       </trans-unit>
