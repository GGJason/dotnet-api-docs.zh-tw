      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Presentation.Hosting.ICommandService">
          <source>Represents a command interface that is used for integration of Windows Workflow Designer actions (for example, context menu), with custom-provided implementations, such as property browser service, debugging service, and so on.</source>
          <target state="translated">表示用於整合 Windows Workflow Designer 動作的命令介面 (例如操作功能表)，其中提供自訂實作，例如屬性瀏覽器服務、偵錯服務等等。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.CanExecuteCommand(System.Int32)">
          <source>The command identifier to be checked.</source>
          <target state="translated">要檢查的命令識別項。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.CanExecuteCommand(System.Int32)">
          <source>Verifies whether the specified command can be executed.</source>
          <target state="translated">確認是否可以執行指定的命令。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.CanExecuteCommand(System.Int32)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the specified command can be executed; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果指定的命令可以執行，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.CanExecuteCommand(System.Int32)">
          <source>The command is not supported.</source>
          <target state="translated">不支援此命令。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.ExecuteCommand(System.Int32,System.Collections.Generic.Dictionary{System.String,System.Object})">
          <source>The command identifier to be checked.</source>
          <target state="translated">要檢查的命令識別項。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.ExecuteCommand(System.Int32,System.Collections.Generic.Dictionary{System.String,System.Object})">
          <source>The parameters of the command.</source>
          <target state="translated">命令的參數。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.ExecuteCommand(System.Int32,System.Collections.Generic.Dictionary{System.String,System.Object})">
          <source>Executes the command with the specified command identifier and parameters.</source>
          <target state="translated">使用指定的命令識別項與參數執行命令。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.ExecuteCommand(System.Int32,System.Collections.Generic.Dictionary{System.String,System.Object})">
          <source>The command is not supported.</source>
          <target state="translated">不支援此命令。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.IsCommandSupported(System.Int32)">
          <source>The command identifier to be checked.</source>
          <target state="translated">要檢查的命令識別項。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.IsCommandSupported(System.Int32)">
          <source>Returns a Boolean value that indicates whether the specified command identifier is supported.</source>
          <target state="translated">傳回布林值，這個值會指出是否支援指定的命令識別項。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Hosting.ICommandService.IsCommandSupported(System.Int32)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the specified command ID is supported; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果支援指定的命令 ID，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
