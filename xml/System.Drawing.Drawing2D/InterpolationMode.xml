      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Drawing.Drawing2D.InterpolationMode">
          <source>The <ph id="ph1">&lt;see cref="T:System.Drawing.Drawing2D.InterpolationMode" /&gt;</ph> enumeration specifies the algorithm that is used when images are scaled or rotated.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Drawing.Drawing2D.InterpolationMode" /&gt;</ph> 列舉型別會指定縮放或旋轉影像時所使用的演算法。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.Bicubic">
          <source>Specifies bicubic interpolation.</source>
          <target state="translated">指定雙立方的插補法。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.Bicubic">
          <source>No prefiltering is done.</source>
          <target state="translated">不做預先篩選。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.Bicubic">
          <source>This mode is not suitable for shrinking an image below 25 percent of its original size.</source>
          <target state="translated">這個模式不適用於將影像縮小到原始尺寸的百分之 25 以下。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.Bilinear">
          <source>Specifies bilinear interpolation.</source>
          <target state="translated">指定雙線形的插補法。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.Bilinear">
          <source>No prefiltering is done.</source>
          <target state="translated">不做預先篩選。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.Bilinear">
          <source>This mode is not suitable for shrinking an image below 50 percent of its original size.</source>
          <target state="translated">這個模式不適用於將影像縮小到原始大小的百分之 50 以下。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.Default">
          <source>Specifies default mode.</source>
          <target state="translated">指定預設模式。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.High">
          <source>Specifies high quality interpolation.</source>
          <target state="translated">指定高品質的插補法。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.HighQualityBicubic">
          <source>Specifies high-quality, bicubic interpolation.</source>
          <target state="translated">指定高品質的雙立方插補法。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.HighQualityBicubic">
          <source>Prefiltering is performed to ensure high-quality shrinking.</source>
          <target state="translated">將執行預先篩選，以確保縮小影像的品質。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.HighQualityBicubic">
          <source>This mode produces the highest quality transformed images.</source>
          <target state="translated">這個模式會產生品質最高的變形影像。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.HighQualityBilinear">
          <source>Specifies high-quality, bilinear interpolation.</source>
          <target state="translated">指定高品質的雙線形插補法。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.HighQualityBilinear">
          <source>Prefiltering is performed to ensure high-quality shrinking.</source>
          <target state="translated">將執行預先篩選，以確保縮小影像的品質。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.Invalid">
          <source>Equivalent to the <ph id="ph1">&lt;see cref="F:System.Drawing.Drawing2D.QualityMode.Invalid" /&gt;</ph> element of the <ph id="ph2">&lt;see cref="T:System.Drawing.Drawing2D.QualityMode" /&gt;</ph> enumeration.</source>
          <target state="translated">等同於 <ph id="ph1">&lt;see cref="F:System.Drawing.Drawing2D.QualityMode.Invalid" /&gt;</ph> 列舉型別的 <ph id="ph2">&lt;see cref="T:System.Drawing.Drawing2D.QualityMode" /&gt;</ph> 項目。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.Low">
          <source>Specifies low quality interpolation.</source>
          <target state="translated">指定低品質的插補法。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="F:System.Drawing.Drawing2D.InterpolationMode.NearestNeighbor">
          <source>Specifies nearest-neighbor interpolation.</source>
          <target state="translated">指定最相鄰的插補法。</target>       </trans-unit>
